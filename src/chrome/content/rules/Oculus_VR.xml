<!--
	At least some pages redirect to http.

-->
<ruleset name="Oculus VR (partial)">

	<target host="oculusvr.com" />
	<target host="*.oculusvr.com" />


	<securecookie host="^developer\.oculusvr\.com$" name=".+" />


	<rule from="^http://(www\.)?oculusvr\.com/(pre-order|wp-content/)"
		to="https://$1oculusvr.com/$2" />

	<rule from="^http://developer\.oculusvr\.com/"
		to="https://developer.oculusvr.com/" />

</ruleset>