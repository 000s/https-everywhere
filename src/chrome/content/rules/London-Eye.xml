<ruleset name="London Eye">
	<target host="londoneye.com" />
	<target host="secure.londoneye.com" />
	<target host="www.londoneye.com" />

	<securecookie host=".*\.londoneye\.com$" name=".+" />

	<!-- Timeout: -->
	<rule from="^http://londoneye\.com/"
		to="https://www.londoneye.com/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
