<ruleset name="jQuery (partial)">

	<target host="jquery.com" />
	<target host="*.jquery.com" />


	<securecookie host="^(?:forum|meetups)\.jquery\.com$" name=".+" />


	<rule from="^http://((?:code|forum|meetups|www)\.)?jquery\.com/"
		to="https://$1jquery.com/" />

</ruleset>
