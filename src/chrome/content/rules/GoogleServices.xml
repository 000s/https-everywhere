		<test url="http://corp.google.com/" />
			<test url="http://login.corp.google.com/" />
			<test url="http://ideas.corp.google.com/" />
			<test url="http://discovery.corp.google.com/" />
		<test url="http://developers.google.com/" />
		<test url="http://dl.google.com/" />
		<test url="http://docs.google.com/" />
			<test url="http://docs1.google.com/" />
			<test url="http://docs2.google.com/" />
			<test url="http://docs3.google.com/" />
			<test url="http://1.docs.google.com/" />
			<test url="http://2.docs.google.com/" />
			<test url="http://3.docs.google.com/" />
		<test url="http://drive.google.com/" />
		<test url="http://encrypted.google.com/" />
			<test url="http://encrypted-tbn1.google.com/" />
			<test url="http://encrypted-tbn2.google.com/" />
			<test url="http://encrypted-tbn3.google.com/" />
		<test url="http://feedburner.google.com/" />
		<test url="http://feedproxy.google.com/" />
		<test url="http://fiber.google.com/" />
		<test url="http://fonts.google.com/" />
		<test url="http://gg.google.com/" />
		<test url="http://glass.google.com/" />
		<test url="http://goto.google.com/" />
		<test url="http://health.google.com/" />
		<test url="http://helpouts.google.com/" />
		<test url="http://history.google.com/" />
		<test url="http://talkgadget.google.com/" />
		<test url="http://hostedtalkgadget.google.com/" />
		<test url="http://investor.google.com/" />
		<test url="http://ipv4.google.com/" />
		<test url="http://knol.google.com/" />
		<test url="http://knoll.google.com/" />
		<test url="http://mail.google.com/" />
			<test url="http://chatenabled.mail.google.com/" />
		<test url="http://pack.google.com/" />
		<test url="http://pki.google.com/" />
		<test url="http://play.google.com/" />
		<test url="http://plus.google.com/" />
			<test url="http://plus.sandbox.google.com/" />
		<test url="http://plusone.google.com/" />
		<test url="http://productforums.google.com/" />
		<test url="http://profiles.google.com/" />
		<test url="http://safebrowsing-cache.google.com/" />
		<test url="http://cert-test.sandbox.google.com/" />
		<test url="http://sb-ssl.google.com/" />
		<test url="http://script.google.com/" />
		<test url="http://security.google.com/" />
		<test url="http://services.google.com/" />
		<test url="http://servicessites.google.com/" />
		<test url="http://sites.google.com/" />
		<test url="http://spreadsheets.google.com/" />
			<test url="http://spreadsheets1.google.com/" />
			<test url="http://spreadsheets2.google.com/" />
			<test url="http://spreadsheets3.google.com/" />
		<test url="http://support.google.com/" />
		<test url="http://talk.google.com/" />
		<test url="http://tools.google.com/" />
		<test url="http://wallet.google.com/" />

	<exclusion pattern="^http://clients[0-9]\.google\.com/ocsp"/>

	<rule from="^http://scholar\.google\.((?:com?\.)?\w{2,3})/intl/"
		to="https://www.google.$1/intl/" />
		<test url="http://scholar.google.ca/intl/" />
		<test url="http://scholar.google.com/intl/" />
		<test url="http://scholar.google.com.au/intl/" />

	<rule from="^http://(?:encrypted-)?tbn(\d)\.google\.com/"
		to="https://encrypted-tbn$1.google.com/" />
		<test url="http://tbn2.google.com/" />
		<test url="http://encrypted-tbn2.google.com/" />
		<test url="http://tbn1.google.com/" />

	<rule from="^http://news\.google\.(?:com?\.)?\w{2,3}/(?:$|news|newshp)"
		to="https://www.google.com/news" />

	<rule from="^http://([^\./]\.)?googlesource\.com/"
		to="https://$1googlesource.com/" />

	<rule from="^http://partner\.googleadservices\.com/"
		 to="https://partner.googleadservices.com/" />

	<rule from="^http://(pagead2|tpc)\.googlesyndication\.com/"
		 to="https://$1.googlesyndication.com/" />

	<!--	!www doesn't exist.
					-->
	<rule from="^http://www\.googletagservices\.com/tag/js/"
		to="https://www.googletagservices.com/tag/js/" />

	<!-- subdomain to path redirects -->
	<rule from="^http://(earth|trends)\.google\.com/"
		 to="https://www.google.com/$1" />
		<test url="http://earth.google.com/" />
		<test url="http://trends.google.com/" />

	<!-- wildcards -->

	<rule from="^http://([^@:\./]+)\.googleusercontent\.com/"
		to="https://$1.googleusercontent.com/" />
		<test url="http://webcache.googleusercontent.com/" />
		<test url="http://foo.googleusercontent.com/" />
		<test url="http://bar.googleusercontent.com/" />

	<rule from="^http://([^/:@\.]+\.)?googlecode\.com/"
		 to="https://$1googlecode.com/" />
		<test url="http://foo.googlecode.com/" />
		<test url="http://bar.googlecode.com/" />
		<test url="http://www.googlecode.com/" />

</ruleset>
