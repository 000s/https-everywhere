<!--	See Zendesk.com-mismatches.xml also
-->
<ruleset name="Zendesk">

	<target host="zendesk.com" />
	<target host="asset0.zendesk.com" />
	<target host="assets.zendesk.com" />
	<target host="de.zendesk.com" />
	<target host="es.zendesk.com" />
	<target host="fr.zendesk.com" />
	<target host="ja.zendesk.com" />
	<target host="pt.zendesk.com" />
	<target host="support.zendesk.com" />
	<target host="university.zendesk.com" />
	<target host="www.zendesk.com" />

	<rule from="^http://([\w\-_]+\.)zendesk\.com/"
		to="https://$1zendesk.com/" />

</ruleset>
