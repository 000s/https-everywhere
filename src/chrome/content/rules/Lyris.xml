<ruleset name="Lyris (partial)">

	<target host="lyris.com"/>
	<target host="www.lyris.com"/>
	<target host="up0.net"/>
	<target host="*.up0.net"/>

	<rule from="^http://(www\.)?lyris\.com/"
		to="https://www.lyris.com/"/>

	<rule from="^http://up0\.net/"
		to="https://www.up0.net/"/>

	<rule from="^http://(.+)\.up0\.net/"
		to="https://$1.up0.net/"/>

</ruleset>
