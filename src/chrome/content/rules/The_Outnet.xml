<!--
	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="The Outnet">

	<target host="theoutnet.com" />
	<target host="*.theoutnet.com" />


	<securecookie host="^\.?www\.theoutnet\.com$" name=".+" />


	<rule from="^https?://(?:www\.)?theoutnet\.com/"
		to="https://www.theoutnet.com/" />

</ruleset>