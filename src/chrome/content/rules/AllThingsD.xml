<!--
	Some pages redirect to http.

-->
<ruleset name="AllThingsD (partial)">

	<target host="allthingsd.com" />
	<target host="*.allthingsd.com" />
		<exclusion pattern="^http://(?:www\.)?allthingsd\.com/(?!favicon\.ico|files/|theme/|wp-content/)" />


	<!--securecookie host="^(.*\.)?allthingsd\.com$" name=".+" /-->


	<rule from="^http://(mediamemo\.|voices\.|www\.)?allthingsd\.com/"
		 to="https://$1allthingsd.com/" />

</ruleset>
