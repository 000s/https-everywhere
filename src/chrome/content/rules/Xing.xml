<ruleset name="Xing">

	<target host="xing.com" />
	<target host="*.xing.com" />
	<target host="x1.xingassets.com" />


	<securecookie host=".*\.xing\.com$" name=".+" />


	<rule from="^http://((?:api|blog|corporate|dev|fbx|footer|groups|info|login|online-reputation|shop|startup-pioniere|touch|www)\.)?xing\.com/"
		to="https://$1xing.com/" />

	<rule from="^http://x1\.xingassets\.com/"
		to="https://x1.xingassets.com/" />

</ruleset>
