<!--
	Other Quantcast rulesets:

		- Quantserve.com.xml

-->
<ruleset name="Quantcast" platform="mixedcontent">

	<target host="quantcast.com" />
	<target host="*.quantcast.com" />


	<securecookie host="^(.*\.)?quantcast\.com$" name=".*" />


	<rule from="^http://(ak\.|www\.)?quantcast\.com/"
		to="https://$1quantcast.com/" />

</ruleset>
