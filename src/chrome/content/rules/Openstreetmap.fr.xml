<ruleset name="Openstreetmap.fr">
    <target host="openstreetmap.fr" />
    <target host="www.openstreetmap.fr" />
    <target host="umap.openstreetmap.fr" />

    <securecookie host="^(www\.|umap\.)?openstreetmap\.fr" name=".+" />

    <rule from="^http:"
            to="https:" />
</ruleset>
