<!--
	CDN buckets:

		- iconfinder.cachefly.net

			- cdn[0-6]

		- wac.3a7b.edgecastcdn.net/...

		- icons3.iconfinder.netdna-cdn.com

			- cdn


	Nonfunctional subdomains:

		- ^		(refused)
		- www		(redirects to http, valid cert)


	Fully covered subdomains:

		- cdn[0-6]

-->
<ruleset name="Iconfinder (partial)" default_off="mismatched, self-signed">

	<target host="*.iconfinder.com" />


	<rule from="^http://cdn([0-6])\.iconfinder\.com/"
		to="https://cdn$1.iconfinder.com/" />

</ruleset>