<!--
	Other GMO rulesets:

		- Click-Sec.com.xml
		- FX_Prime.com.xml
		- GlobalSign.xml
		- GMO_Cloud.com.xml
		- GMO_Cloud.us.xml
		- GMO_Registry.com.xml
		- Hello.tokyo.xml
		- InterQ.or.jp.xml
		- Isle.jp.xml
		- Quicktranslate.com.xml
		- RapidSite.jp.xml


	Nonfunctional subdomains:

		- error
		- cache.im	(reset)


	Problematic subdomains:

		- img *

	* Refused


	Fully covered subdomains:

		- cache.img
		- img		(-> cache.img)
		- secure
		- support

-->
<ruleset name="GMO Internet (partial)">

	<target host="gmo.jp" />
	<target host="*.gmo.jp" />


	<securecookie host="^mng\.gmo\.jp$" name=".+" />


	<rule from="^http://((?:mng|secure|support|www)\.)?gmo\.jp/"
		to="https://$1gmo.jp/" />

	<rule from="^http://(?:cache\.)?img\.gmo\.jp/"
		to="https://cache.img.gmo.jp/" />

</ruleset>
