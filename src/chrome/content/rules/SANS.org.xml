<!--
	Other SANS Institute rulesets:

		- GIAC.org.xml
		- SANS.edu.xml


	Fully covered subdomains:

		- (www.)
		- isc

-->
<ruleset name="SANS.org">

	<target host="sans.org" />
	<target host="*.sans.org" />


	<securecookie host="^\.sans\.org$" name=".+" />


	<rule from="^http://(isc\.|www\.)?sans\.org/"
		to="https://$1sans.org/" />

</ruleset>
