<!--
    Not covered:
        selfhtml.org -> self-signed
		blog.selfhtml.org -> redirects to HTTP
		
	Mixed content:
		wiki.selfhtml.org
-->
<ruleset name="SelfHTML">
    <target host="wiki.selfhtml.org" />
    <target host="forum.selfhtml.org" />

    <securecookie host="^(wiki\.|forum\.)?selfhtml\.org$" name=".+" />

    <rule from="^http:"
            to="https:" />
</ruleset>
