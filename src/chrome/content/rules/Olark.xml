<ruleset name="Olark">

	<target host="olark.com" />
	<target host="*.olark.com" />


	<securecookie host="^(.+\.)?olark\.com$" name=".*" />


	<rule from="^http://(?:www\.)?olark\.com/"
		to="https://www.olark.com/" />

	<rule from="^http://(assets|static|\d+-async)\.olark\.com/"
		to="https://$1.olark.com/" />

</ruleset>
