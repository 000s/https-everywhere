<ruleset name="4Shared (buggy)" default_off="Mostly broken">
  <!-- this ruleset is mostly worthless, because www.4shared.com is broken,
  though it does usefully protect some downloads.  Needs A Lot of Work... -->
  <target host="4shared.com"/>
  <target host="*.4shared.com"/>

  <rule from="^http://4shared\.com/" to="https://www.4shared.com/"/>
  <rule from="^http://([^/:@]*)\.4shared\.com/" to="https://$1.4shared.com/"/>
</ruleset>
