<!--
	Nonfunctional subdomains:

		- www	(refused)


	Fully covered subdomains:

		- federation
		- fedregistry
		- groupes
		- services
		- services-federation
		- sourcesup


	^renater.fr does not exist

-->
<ruleset name="Renater.fr (partial)">

	<target host="*.renater.fr" />


	<securecookie host="^(?:groupes|services|sourcesup)\.renater\.fr$" name=".+" />


	<rule from="^http://(federation|fedregistry|groupes|services|services-federation|sourcesup)\.renater\.fr/"
		to="https://$1.renater.fr/" />

</ruleset>