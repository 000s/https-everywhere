<ruleset name="Voxel.net">

	<target host="voxel.net" />
	<target host="portal.voxel.net" />
	<target host="www.voxel.net" />
	<target host="*.voxcdn.com" />

	<rule from="^http://(www\.)?(portal\.)?voxel\.net/"
		to="https://$2voxel.net/" />

	<rule from="^http://([1-9].*)\.voxcdn\.com/"
		to="https://$1.voxcdn.com/ />
</ruleset>
