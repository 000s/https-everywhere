<ruleset name="linuxhostsupport.com">

	<target host="linuxhostsupport.com" />
	<target host="www.linuxhostsupport.com" />


	<!--	^ redirects to www over http, so
		copy that behavior:
					-->
	<rule from="^http://(?:www\.)?linuxhostsupport\.com/"
		to="https://www.linuxhostsupport.com/" />

</ruleset>
