<ruleset name="GOV.UK">

	<target host="gov.uk" />
	<target host="*.gov.uk" />
	<target host="*.businesslink.gov.uk" />


	<rule from="^http://((?:online|www)\.businesslink\.|www\.)?gov\.uk/"
		to="https://$1gov.uk/" />

	<rule from="^https?://businesslink\.gov\.uk/"
		to="https://www.businesslink.gov.uk/" />

</ruleset>
