<!--
	For other Yelp coverage, see Yelp.xml.


	Problematic hosts in *yelp.com.br:

		- (www.)? ᵀ
		- biz ᵀ
		- m ᵀ

	ᵀ Blocks Tor users

-->
<ruleset name="Yelp.com.br" default_off="needs clearnet testing">

	<target host="yelp.com.br" />
	<target host="biz.yelp.com.br" />
	<target host="m.yelp.com.br" />
	<target host="www.yelp.com.br" />


	<securecookie host="^\." name="^(?:__cfduid|cf_clearance)$" />
	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
