<!--	livefyre-avatar.s3.amazonaws.com
-->
<ruleset name="Livefyre (partial)" default_off="mismatch">

	<!--	gridserver.com	-->
	<target host="blog.livefyre.co"/>
	<!--	totemapp.com	-->
	<target host="press.livefyre.co"/>
		<exclusion pattern="^http://press\.livefyre\.co/$"/>
	<!--	cloudflare.net	-->
	<target host="zore.livefyre.co"/>

	<rule from="^http://(blog|press|zor)\.livefyre\.co/"
		to="https://$1.livefyre.co/"/>

</ruleset>
