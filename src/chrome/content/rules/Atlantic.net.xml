<ruleset name="Atlantic.net">
    <target host="atlantic.net" />
    <target host="*.atlantic.net" />

    <securecookie host="^(.*\.)?atlantic\.net$" name=".+" />

    <rule from="^https?://atlantic\.net/"
        to="https://www.atlantic.net/" />
    <rule from="^http://([^/:@]+)?\.atlantic\.net/"
        to="https://$1.atlantic.net/" />
</ruleset>
