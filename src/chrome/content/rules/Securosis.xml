<ruleset name="Securosis">
	<target host="securosis.com" />
	<target host="*.securosis.com" />


	<securecookie host="^(?:www)?\.securosis\.com$" name=".+" />


	<rule from="^http://www\.securosis\.com/" to="https://www.securosis.com/"/>
	<rule from="^http://securosis\.com/" to="https://securosis.com/"/>
</ruleset>

