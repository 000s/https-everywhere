<ruleset name="Register.com">
	<target host="register.com" />
	<target host="help.register.com" />
	<target host="support.register.com" />
	<target host="webmail01.register.com" />
	<target host="webmail02.register.com" />
	<target host="www.register.com" />

	<securecookie host="^((help|support|webmail0[12]|www)?\.)?register\.com$" name=".+" />

	<rule from="^(http://(www\.)?|https://)register\.com/" to="https://www.register.com/" />
	<rule from="^http://(help|support|webmail0[12])\.register\.com/" to="https://$1.register.com/" />
</ruleset>