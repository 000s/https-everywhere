<ruleset name="MIT (mismatches)" default_off="expired, mismatch, self-signed">

	<!--	Expired, self-signed	-->
	<target host="pdos.csail.mit.edu" />
	<!--	cert: *.scripts.mit.edu		-->
	<target host="hacks.mit.edu"/>
	<target host="mosh.mit.edu"/>


	<rule from="^http://pdos\.csail\.mit\.edu/"
		to="https://pdos.csail.mit.edu/" />

	<rule from="^http://(hacks|mosh)\.mit\.edu/"
		to="https://$1.mit.edu/"/>

</ruleset>
