<!--
	CDN buckets:

		- canvas-dynamic-assets.s3.amazonaws.com

-->
<ruleset name="Canvas">

	<target host="canv.as" />
	<target host="*.canv.as" />
	<target host="i.canvasugc.com" />


	<rule from="^http://(www\.)?canv\.as/"
		to="https://$1canv.as/" />

	<rule from="^http://i\.canvasugc\.com/"
		to="https://i.canvasugc.com/" />

</ruleset>