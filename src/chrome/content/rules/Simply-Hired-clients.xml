<ruleset name="Joyent (mismatches)" default_off="mismatch">

	<!--	*.jobamatic.com	-->
	<target host="jobs.nodejs.org"/>

	<!--	is cross-domain cookie used on unsecure pages?	-->
	<securecookie host="^jobs\.nodejs\.org$" name=".*"/>

	<rule from="^http://jobs\.nodejs\.org/"
		to="https://jobs.nodejs.org/"/>

</ruleset>
