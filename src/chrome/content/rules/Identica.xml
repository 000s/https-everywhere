<ruleset name="Identica">
  <target host="*.identi.ca" />
  <target host="identi.ca" />
  <target host="files.status.net" />

  <rule from="^http://(?:www\.)?identi\.ca/"
	        to="https://identi.ca/"/>
  <rule from="^http://files\.status\.net/"
	        to="https://files.status.net/"/>
  <rule from="^http://(meteor\d+\.identi\.ca)/" 
          to="https://$1/" />
</ruleset>
