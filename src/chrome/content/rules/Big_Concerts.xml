<!--
	Some (most?) pages redirect to http

-->
<ruleset name="Big Concerts (partial)" default_off="expired, mismatch">

	<target host="bigconcerts.co.za" />
	<target host="www.bigconcerts.co.za" />


	<rule from="^http://(www\.)?bigconcerts\.co\.za/(account|media/|resource/|themes/|ui/|zpimages/)"
		to="https://$1bigconcerts.co.za/$2" />

</ruleset>
