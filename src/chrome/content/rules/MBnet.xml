<ruleset name="MBnet">
  <target host="www.mbnet.fi" />
  <target host="mbnet.fi" />
  <target host="mbi.mbnet.fi" />
  <target host="webmail.mbnet.fi" />

  <rule from="^http://(www\.)?mbnet\.fi/" to="https://www.mbnet.fi/"/>
  <rule from="^http://mbi\.mbnet\.fi/" to="https://mbi.mbnet.fi/" />
  <rule from="^http://webmail\.mbnet\.fi/" to="https://webmail.mbnet.fi/" />
</ruleset>
