<!--
	For rules causing false/broken MCB, see Tcl.tk-falsemixed.xml.


	Mixed content:

		- css from $self *
		- Images from $self *

	* Secured by us

-->
<ruleset name="Tcl.tk (partial)">

	<target host="tcl.tk" />
	<target host="www.tcl.tk" />
		<!--
			Avoid broken MCB:
						-->
		<exclusion pattern="^http://(?:www\.)?tcl\.tk/+(?!devsite\.css|images/)" />

			<test url="http://www\.tcl\.tk/devsite.css" />


	<rule from="^http:"
		to="https:" />

</ruleset>
