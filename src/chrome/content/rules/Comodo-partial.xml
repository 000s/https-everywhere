<!--
	Comodo sites that have mixed content
	See also Comodo.xml
-->
<ruleset name="Comodo (partial)" platform="mixedcontent">
	<target host="forums.comodo.com"/>

	<securecookie host="^forums\.comodo\.com$" name=".+"/>

	<rule from="^http://forums\.comodo\.com/"
		to="https://forums.comodo.com/" />
</ruleset>
