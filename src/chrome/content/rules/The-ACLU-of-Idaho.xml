<ruleset name="The ACLU of Idaho">
	<target host="acluidaho.org" />
	<target host="www.acluidaho.org" />

	<securecookie host="^acluidaho\.org$"
			name=".+" />

	<test url="http://acluidaho.org/" />
	<test url="http://www.acluidaho.org/" />
	<test url="https://www.acluidaho.org/" />

	<rule from="^(http://(www\.)?|https://www\.)acluidaho\.org/"
		to="https://acluidaho.org/" />
</ruleset>