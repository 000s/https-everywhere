<!--
	Other CZ.NIC rulesets:

		- DNSSEC-Validator.cz.xml
		- Knot-DNS.cz.xml
		- MojeID.cz.xml


	Fully covered subdomains:

		- (www.)
		- blog
		- git
		- labs
		- gitlab.labs
		- lists
		- piwik


	Observed cookie domains:

		- .
		- .labs *
		- stats ยน
		- www ยน

	ยน Not secured by server
	* Tracking cookies only

-->
<ruleset name="CZ.NIC">

	<target host="nic.cz" />
	<target host="*.nic.cz" />


	<securecookie host="^((?:www|labs|git|blog)?\.)?nic\.cz$" name=".+" />


	<rule from="^http://((?:www|labs|git|blog|gitlab\.labs|lists|piwik)\.)?nic\.cz/"
		to="https://$1nic.cz/" />

</ruleset>
