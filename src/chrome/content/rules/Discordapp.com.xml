<ruleset name="Discordapp.com">
	<target host="discordapp.com" />
	<target host="*.discordapp.com" />
	<test url="www.discordapp.com" />
	<test url="support.discordapp.com" />
	<test url="status.discordapp.com" />
	<test url="blog.discordapp.com" />
	<test url="feedmach.discordapp.com" />
	<test url="merch.discordapp.com" />
	<test url="i18n.discordapp.com" />
	
	<rule from="^http:" to="https:" />
</ruleset>
