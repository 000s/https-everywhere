<ruleset name="Songkick">

	<target host="songkick.com"/>
	<target host="www.songkick.com"/>
	<target host="accounts.songkick.com"/>

	<securecookie host="^(?:.*\.)?songkick.com$" name=".*"/>

	<rule from="^http://(www\.)?songkick\.com/" to="https://www.songkick.com/"/>
	<rule from="^http://accounts\.songkick\.com/" to="https://accounts.songkick.com/"/>	

</ruleset>
