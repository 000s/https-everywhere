<!--
	Fully covered subdomains:

		- (www.)buildsecurityin
		- preview.buildsecurityin

-->
<ruleset name="US-Cert.gov">

	<target host="us-cert.gov" />
	<target host="*.us-cert.gov" />


	<rule from="^http://((?:(?:preview\.|www\.)?buildsecurityin|ics-cert|www)\.)?us-cert\.gov/"
		to="https://$1us-cert.gov/" />

</ruleset>
