<ruleset name="Eole Water" default_off="mismatch, self-signed">

	<!--	cert: r35894.ovh.net
	r35894.ovh.net redirects to eolewater		-->
	<target host="eolewater.com"/>
	<target host="www.eolewater.com"/>

	<rule from="^http://(?:www\.)?eolewater\.com/"
		to="https://www.eolewater.com/"/>

</ruleset>
