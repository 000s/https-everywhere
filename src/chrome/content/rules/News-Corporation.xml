<!--	See WashingtonPost-mismatches.xml also
-->
<ruleset name="News Corporation (partial)" default_off="broken">

	<target host="fins.com"/>
	<target host="www.fins.com"/>
	<target host="secure.nypost.com"/>
	<target host="s2.wsj.net"/>
	<target host="sc.wsj.net"/>

	<securecookie host="^secure.nypost.com$" name=".*"/>

	<rule from="^http://(?:www\.)?fins\.com/Finance/css/"
		to="https://www.fins.com/Finance/css/"/>

	<rule from="^http://secure\.nypost\.com/"
		to="https://secure.nypost.com/"/>

	<rule from="^http://s([2c])\.wsj\.net/"
		to="https://s$1.wsj.net/"/>

</ruleset>
