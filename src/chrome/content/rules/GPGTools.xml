<!--
	Unsupported subdomains:

	support	(certificate name mismatch)
-->
<ruleset name="GPGTools (partial)">
	<target host="gpgtools.org" />
	<target host="*.gpgtools.org" />

    <rule from="^http://gpgtools\.org/"
        to="https://gpgtools.org/" />
    <rule from="^http://([^/:@]+\.)?gpgtools\.org/"
        to="https://$1gpgtools.org/" />
</ruleset>
