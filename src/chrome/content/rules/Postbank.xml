<ruleset name="Postbank">
  <target host="postbank.de"/>
  <target host="*.postbank.de"/>
  <target host="banking.postbank.de"/>

  <securecookie host="^(.*\.)?postbank\.de$" name=".*" />

  <rule from="^http://(tp\.|www\.)?postbank\.de/" to="https://$1postbank.de/"/>
  <rule from="^http://banking\.postbank\.de/" to="https://banking.postbank.de/"/>
</ruleset>
