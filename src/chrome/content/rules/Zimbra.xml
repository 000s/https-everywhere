<ruleset name="Zimbra">

	<target host="zimbra.com" />
	<target host="*.zimbra.com" />


	<securecookie host="^(.*\.)?zimbra\.com$" name=".*" />


	<!--	files2: Akamai
		Data appear identical.	-->
	<rule from="^http://files2?\.zimbra\.com/"
		to="https://s3.amazonaws.com/files.zimbra.com/" />

	<rule from="^http://([^/:@\.]+\.)?zimbra\.com/"
		to="https://$1zimbra.com/"/>

</ruleset>

