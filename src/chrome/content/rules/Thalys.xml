<ruleset name="Thalys">
    <target host="thalys.com" />
    <target host="www.thalys.com" />

    <securecookie host="^(.*\.)?thalys\.com$" name=".+" />

    <rule from="^https?://thalys\.com/"
        to="https://www.thalys.com/" />
    <rule from="^http://([^/:@]+)?\.thalys\.com/"
        to="https://$1.thalys.com/" />
</ruleset>
