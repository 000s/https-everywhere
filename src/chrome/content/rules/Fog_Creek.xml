<!--
	Other Fog Creek rulesets:

		- Trello.xml

-->
<ruleset name="Fog Creek">

	<target host="fogcreek.com" />
	<target host="*.fogcreek.com" />


	<securecookie host="^(?:secure\.|\.)?fogcreek\.com$" name=".+" />


	<rule from="^http://(secure\.|www\.)?fogcreek\.com/"
		to="https://$1fogcreek.com/" />

</ruleset>