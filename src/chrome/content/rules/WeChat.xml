<!--
	Nonfunctional domains:

		- support.wechat.com	(mismatched, CN: weixin.qq.com)


	Problematic domains:

		- www.wechat.com	(mismatched, CN: www.wechatapp.com)

-->
<ruleset name="WeChat (partial)">

	<target host="wechat.com" />
	<target host="*.wechat.com" />


	<rule from="^https?://(?:www\.)?wechat\.com/"
		to="https://wechat.com/" />

</ruleset>