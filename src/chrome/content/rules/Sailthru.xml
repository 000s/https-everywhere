<ruleset name="Sailthru">

	<target host="sailthru.com"/>
	<target host="*.sailthru.com"/>

	<!--	Observed cookie domains:

			- .getstarted
			- my
					-->
	<securecookie host=".+\.getstarted\.com$" name=".*"/>

	<!--	Observed domains:
			- ak
			- blog
			- cb
			- getstarted
			- my
			- www		-->
	<rule from="^http://(\w*\.)?sailthru\.com/"
		to="https://$1sailthru.com/"/>

</ruleset>
