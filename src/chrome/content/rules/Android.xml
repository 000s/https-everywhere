<!--
	Fully covered subdomains:

		- (www.)
		- developer
		- market
		- source

-->
<ruleset name="Android">

	<target host="android.com" />
	<target host="*.android.com" />


	<securecookie host=".+\.android\.com$" name=".+" />


	<rule from="^http://((?:developer|market|source)\.)?android\.com/"
		to="https://$1android.com/" />

</ruleset>
