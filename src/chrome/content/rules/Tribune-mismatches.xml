<ruleset name="Tribune (mismatches)" default_off="mismatch">

	<!--	Akamai	-->
	<target host="baltimoresun.com" />
	<target host="www.baltimoresun.com" />


	<rule from="^http://(?:www\.)?baltimoresun\.com/"
		to="https://www.baltimoresun.com/" />

</ruleset>
