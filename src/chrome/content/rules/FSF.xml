<ruleset name="Free Software Foundation">

	<target host="fsf.org" />
	<target host="*.fsf.org" />
	<target host="gnu.org" />
	<target host="*.gnu.org" />
        <target host="*.nongnu.org" />

        
	<rule from="^http://(?:www\.)?fsf\.org/"
		to="https://www.fsf.org/" />

	<rule from="^http://(crm|static)\.fsf\.org/"
		to="https://$1.fsf.org/" />

        <rule from="^http://(?:www\.)?gnu\.org/"
		to="https://www.gnu.org/" />

        <rule from="^http://(lists|savannah)\.(non)?gnu\.org/"
		to="https://$1.$2gnu.org/" />

</ruleset>
