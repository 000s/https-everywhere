<ruleset name="Rapid7 (partial)">

	<target host="rapid7.com" />
	<target host="*.rapid7.com" />


	<securecookie host="^(?:community|www)\.rapid7\.com$" name=".+" />


	<rule from="^http://(browsercan\.|community\.|www\.)?rapid7\.com/"
		to="https://$1rapid7.com/" />

</ruleset>
