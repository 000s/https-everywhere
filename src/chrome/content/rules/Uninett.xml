<!--
	Other Uninett rulesets:

		- Discojuice.org.xml
		- Feide.xml
		- Foodl.org.xml


	Fully covered subdomains:

		- (www.)
		- openwiki
		- tjenester
		- test.tjenester
		- www-feide

-->
<ruleset name="Uninett (partial)">

	<target host="uninett.no" />
	<target host="*.uninett.no" />


	<securecookie host="^(?:openwiki|\.(?:test\.)?tjenester|\.www)\.uninett\.no$" name=".+" />


	<rule from="^http://((?:openwiki|(?:test\.)?tjenester|www|www-feide)\.)?uninett\.no/"
		to="https://$1uninett.no/" />

</ruleset>