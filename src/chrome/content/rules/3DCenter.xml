<ruleset name="3DCenter.org (cacert)" platform="cacert">
	<target host="www.3dcenter.org" />
	<target host="m.3dcenter.org" />
	<target host=".3dcenter.org" /><!-- .3dcenter.org is used as the cookie domain -->
	<rule from="^http://www\.3dcenter\.org/" to="https://www.3dcenter.org/"/>
	<rule from="^http://m\.3dcenter\.org/" to="https://m.3dcenter.org/"/>
	<securecookie host="^(www|m)?\.3dcenter\.org$" name="^SESS.+" />
</ruleset>
