<ruleset name="Stop the Data Retention" default_off="CACert, mismatch">

	<!--	all: cert: wiki.vorratsdatenspeicherung.de, www.freiheitsredner.de	-->
	<target host="vorratsdatenspeicherung.de"/>
	<target host="*.vorratsdatenspeicherung.de"/>

	<rule from="^http://(?:www\.)?(wiki\.)?vorratsdatenspeicherung\.de/"
		to="https://$1vorratsdatenspeicherung.de/"/>

</ruleset>
