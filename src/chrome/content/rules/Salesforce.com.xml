<ruleset name="Desk.com (partial)">

	<target host="webassets.assistly.com"/>
	<target host="desk.com"/>
	<target host="dev.desk.com"/>
	<target host="reg.desk.com"/>
	<target host="support.desk.com"/>
	<target host="www.desk.com"/>
	<target host="webassets.desk.com"/>
	<target host="dreamevent.secure.force.com"/>
	<target host="salesforce.com"/>
	<target host="login.salesforce.com"/>
	<target host="www.salesforce.com"/>
	<target host="secure2.sfdcstatic.com"/>

	<rule from="^http://webassets\.assistly\.com/"
		to="https://assistlywebsite.s3.amazonaws.com/"/>

	<rule from="^http://(dev|reg|support)\.desk\.com/"
		to="https://$1.desk.com/"/>

	<rule from="^http://webassets\.desk\.com/"
		to="https://s3.amazonaws.com/deskwww/"/>

	<rule from="^http://(www\.)?desk\.com/"
		to="https://www.desk.com/"/>

	<rule from="^http://dreamevent\.secure\.force\.com/"
		to="https://dreamevent.secure.force.com/"/>

	<rule from="^http://(www\.)?salesforce\.com/"
		to="https://www.salesforce.com/"/>

	<rule from="^http://login\.salesforce\.com/"
		to="https://login.salesforce.com/"/>

	<rule from="^http://(www\.)?salesforce\.com/(assets|common)/"
		to="https://secure2.sfdcstatic.com/$2/"/>

	<rule from="^http://(secure|www)2\.sfdcstatic\.com/"
		to="https://secure2.sfdcstatic.com/"/>

</ruleset>
