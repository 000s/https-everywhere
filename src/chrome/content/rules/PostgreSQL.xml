<ruleset name="PostgreSQL">
  <target host="commitfest.postgresql.org" />
  <target host="wiki.postgresql.org" />
  <target host="planet.postgresql.org" />
  <target host="redmine.postgresql.org" />
  <target host="nagios.postgresql.org" />

  <rule from="^http://((wiki|commitfest|planet|redmine|nagios)\.postgresql\.org)/" to="https://$1/"/>
  <!--
  As of 2012-02-09, known unprotected subdomains are:
  www.postgresql.org
  archives.postgresql.org
  git.postgresql.org
  yum.postgresql.org
  ftp.postgresql.org
  webmail.postgresql.org
  *.projects.postgresql.org
  babel.postgresql.org
  jdbc.postgresql.org
  doxygen.postgresql.org
  media.postgresql.org
  buildfarm.postgresql.org
  -->
</ruleset>
