<ruleset name="PriceAngels">

	<target host="priceangels.com" />
	<target host="*.priceangels.com" />


	<securecookie host="^\.?priceangels\.com$" name=".+" />


	<rule from="^https?://(?:www\.)?priceangels\.com/"
		to="https://priceangels.com/" />

</ruleset>