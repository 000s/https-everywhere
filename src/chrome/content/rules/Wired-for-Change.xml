<ruleset name="Wired for Change">
	<target host="wiredforchange.com" />
	<target host="*.wiredforchange.com" />

	<securecookie host="^(.*\.)?wiredforchange\.com$" name=".*"/>

	<rule from="^http://([a-zA-Z0-9\-]+)\.wiredforchange\.com/" to="https://$1.wiredforchange.com/" />
	<rule from="^http://wiredforchange\.com/" to="https://wiredforchange.com/" />
</ruleset>
