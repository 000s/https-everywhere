<ruleset name="Geek.net (mismatches)" default_off="mismatch">

	<!--	shareholder.com	-->
	<target host="investors.geek.net"/>
	<target host="jobs.slashdot.org"/>

	<rule from="^http://investors\.geek\.net/common/"
		to="https://investors.geek.net/common/"/>

	<rule from="^http://jobs\.slashdot\.org/"
		to="https://jobs.slashdot.org/"/>

</ruleset>
