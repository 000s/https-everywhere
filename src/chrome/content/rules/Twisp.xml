<ruleset name="twisp">

	<target host="twisp.*" />
	<target host="*.twisp.io" />
	<target host="*.twisp.ly" />
	<target host="*.twisp.me" />


	<securecookie host="^(?:w*\.)?twisp\.(?:io|ly|me)$" name=".+" />


	<rule from="^http://(www\.)?twisp\.(io|ly|me)/"
		to="https://$1twisp.$2/" />

</ruleset>