<!--	for Zendesk clients which otherwise have no rules
-->
<ruleset name="Zendesk.com clients (partial)" default_off="mismatch">

	<target host="support.simplyhired.com"/>
	<target host="support.voxer.com"/>

	<securecookie host="^support\.(simplyhired|voxer)\.com$" name=".*"/>

	<rule from="^http://support\.(simplyhired|voxer)\.com/(generated|images|system)/"
		to="https://support.$1.com/$2/"/>

</ruleset>
