<ruleset name="Live Nation Entertainment (mismatches)" default_off="expired, mismatch">

	<target host="bigconcerts.co.za" />
	<target host="www.bigconcerts.co.za" />
	<target host="roadlangel.co.za" />
	<target host="www.roadlangel.co.za" />


	<rule from="^http://(?:www\.)?(?:bigconcerts|roadangel)\.co\.za/"
		to="https://www.roadangel.co.za/" />

</ruleset>
