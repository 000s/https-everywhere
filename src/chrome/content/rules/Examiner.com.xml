<!--	uses data from stats.examiner.com that doesnt allow https with a correct cert. 2o7.net
	Uses b.scorecardresearch.com, that only provides ssl via akamai. -->

<ruleset name="examiner.com">

	<target host="examiner.com"/>
	<target host="cdn2-b.examiner.com"/>
	<target host="www.examiner.com"/>

	<rule from="^http://(cdn2-b\.|www\.)examiner\.com/"
		to="https://$1examiner.com/"/>

</ruleset>

