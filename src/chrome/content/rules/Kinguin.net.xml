<!--
Mismatch:
https://ladypopular.kinguin.net/
https://khanwars.kinguin.net/

Timeout:
https://team.kinguin.net
-->

<ruleset name="Kinguin.net">
  <target host="kinguin.net" />
  <target host="www.kinguin.net" />
  <exclusion pattern="^http://(www\.)?ladypopular.kinguin.net/" />
          <test url="http://www.ladypopular.kinguin.net/" />
          <test url="http://ladypopular.kinguin.net/" />
  <exclusion pattern="^http://(www\.)?khanwars.kinguin.net/" />
          <test url="http://www.khanwars.kinguin.net/" />
          <test url="http://khanwars.kinguin.net/" />
  <exclusion pattern="^http://(www\.)?team.kinguin.net/" />
          <test url="http://www.team.kinguin.net/" />
          <test url="http://team.kinguin.net/" />
          
  <rule from="^http:" to="https:" />
</ruleset>
