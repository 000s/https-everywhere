<ruleset name="Two Factor Auth">
    <target host="twofactorauth.org" />
    <target host="*.twofactorauth.org" />

    <securecookie host="^(.*\.)?twofactorauth\.org$" name=".*" />

    <rule from="^http://twofactorauth\.org/"
        to="https://twofactorauth.org/" />
    <rule from="^http://([^/:@]+)\.twofactorauth\.org/"
        to="https://$1.twofactorauth.org/" />
</ruleset>
