<!--
	For Blender.org

		Works:

			- www
			- $
			- developer
			- projects

		Doesn't Work:
			- wiki**

		* Dropped
		** Refused


	These altnames don't exist:

		- www.projects.blender.org

-->
<ruleset name="Blender.org">

	<target host="*.blender.org" />

	<!-- Secured by server: -->
	<!--securecookie host="^\.developer\.blender\.org$" name="^phsid$" /-->

	<rule from="^http://((?:www\.)?developer|projects)\.blender\.org/"
		  to="https://$1.blender.org/" />

	<rule from="^http://(?:www\.)blender\.org/"
	      to="https://www.blender.org/" />

	<test url="http://blender.org/" />
	<test url="http://www.blender.org/" />
	<test url="http://developer.blender.org/" />
	<test url="http://projects.blender.org/" />
	<test url="http://www.projects.blender.org/" />
	<test url="http://www.developer.blender.org/" />

</ruleset>
