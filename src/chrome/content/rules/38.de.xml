<!--
	Problematic subdomains:

		- ^

			- [^?].* redirects to login 


	Mixed content:

		Videos from www.youtube-nocookie.com

-->
<ruleset name="38.de" platform="mixedcontent">
  <target host="*.38.de" />
  <target host="38.de" />


	<securecookie host="^\.38\.de$" name=".+" />


	<!--	Server drops path:
					-->
	<rule from="^http://38\.de/.*"
		to="https://www.38.de/" />

  <rule from="^http://www\.38\.de/" to="https://www.38.de/"/>
</ruleset>
