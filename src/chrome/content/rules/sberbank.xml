<!--
	Mismatch:
		- pvb.sbrf.ru
		- beta.sberbank.ru

	RC4 only:
		- ok.sberbank.ru
-->
<ruleset name="sberbank">
	<target host="sbrf.ru" />
	<target host="www.sbrf.ru" />
	<target host="sberbank.ru" />
	<target host="www.sberbank.ru" />
	<target host="online.sberbank.ru" />
	<target host="sbi.sberbank.ru:9443" />

	<rule from="^http:"
		to="https:" />
</ruleset>
