<!--
	For problematic rules, see Fanhatten-mismatches.xml.

-->
<ruleset name="Fanhatten (partial)">

	<target host="fanhattan.com" />
	<target host="*.fanhattan.com" />


	<!--	- !www times out over https
		- !www redirects to www over http
			-->
	<rule from="^https?://(?:www\.)?fanhatten\.com/"
		to="https://www.fanhatten.com/" />

	<rule from="^http://blog\.fanhatten\.com/"
		to="https://blog.fanhatten.com/" />

</ruleset>
