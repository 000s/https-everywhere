<ruleset name="Educause">

	<target host="educause.edu" />
	<target host="*.educause.edu" />


	<rule from="^http://(net\.)?educause\.edu/"
		to="https://$1educause.edu/" />

	<rule from="^http://www\.educause\.edu/"
		to="https://www.educause.edu/" />

</ruleset>
