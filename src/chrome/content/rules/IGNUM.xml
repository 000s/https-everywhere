<ruleset name="IGNUM (partial)">

	<target host="domena.cz" />
	<target host="www.domena.cz" />
	<target host="ignum.cz" />
	<target host="www.ignum.cz" />


	<rule from="^http://(www\.)?domena\.cz/"
		to="https://$1domena.cz/" />

	<rule from="^http://(www\.)?ignum\.cz/"
		to="https://$1ignum.cz/" />

</ruleset>
