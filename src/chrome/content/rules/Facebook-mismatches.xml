<!--
	For rules that are on by default, see Facebook.xml.

-->
<ruleset name="Facebook (mismatches)" default_off="mismatch">

	<!--	Akamai	-->
	<target host="sphotos.ak.fbcdn.net" />
	<target host="*.sphotos.ak.fbcdn.net" />


	<rule from="^https?://(?:a\d\.)?sphotos\.ak\.fbcdn\.net/"
		to="https://sphotos.ak.fbcdn.net/" />

</ruleset>
