<ruleset name="Canonical (partial)">

	<target host="*.canonical.com" />


	<securecookie host="^(?:certification|shop)\.canonical\.com$" name=".+" />


	<rule from="^http://(blog\.bazaar|certification|design|shop)\.canonical\.com/"
		to="https://$1.canonical.com/" />

</ruleset>