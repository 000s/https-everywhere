<!--
	CDN buckets:

		- s3.amazonaws.com/totem_production/

-->
<ruleset name="Totem">

	<target host="totemapp.com" />
	<target host="*.totemapp.com" />


	<securecookie host="^(.*\.)?totemapp\.com$" name=".*" />


	<rule from="^https?://(?:www\.)?totemapp\.com/"
		to="https://www.totemapp.com/"/>

	<rule from="^http://totem\.totemapp\.com/"
		to="https://totem.totemapp.com/"/>

</ruleset>
