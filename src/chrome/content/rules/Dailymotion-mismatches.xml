<!--
	For rules that are on by default, see Dailymotion.xml.

-->
<ruleset name="Dailymotion (mismatches)" default_off="expired, mismatch, self-signed">

	<!--	Cert: files.dailymotion.com, self-signed, expired	-->
	<target host="press.dailymotion.com" />
	<!--	Cert: mediaplan.ovh.net	-->
	<target host="publisher.dailymotion.com" />


	<!--	Cookies are handled in Dailymotion.xml.	-->


	<rule from="^http://(press|publisher)\.dailymotion\.com/"
		to="https://$1.dailymotion.com/" />

</ruleset>
