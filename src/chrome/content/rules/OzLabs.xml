<!--
	Nonfunctional subdomains:

		- jk *
		- patchwork	(shows www)

	* Shows www

-->
<ruleset name="OzLabs (CAcert)" platform="cacert">

	<target host="ozlabs.org"/>
	<target host="*.ozlabs.org"/>
  <exclusion pattern="^http://(git|jk|librtas|patchwork|powerpc-utils|yaboot)\.ozlabs\.org/"/>

	<rule from="^http://ozlabs\.org/"
          to="https://ozlabs.org/"/>

	<rule from="^http://([a-zA-Z0-9\-\.]+)\.ozlabs\.org/"
          to="https://$1.ozlabs.org/"/>

</ruleset>
