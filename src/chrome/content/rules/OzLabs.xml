<ruleset name="OzLabs" default_off="CACert">

	<target host="ozlabs.org"/>
	<target host="*.ozlabs.org"/>
		<exclusion pattern="^http://(git|librtas|patchwork|powerpc-utils|yaboot)\.ozlabs\.org/"/>

	<rule from="^http://ozlabs\.org/"
		to="https://ozlabs.org/"/>

	<rule from="^http://([\w\-_]+)\.ozlabs\.org/"
		to="https://$1.ozlabs.org/"/>

</ruleset>
