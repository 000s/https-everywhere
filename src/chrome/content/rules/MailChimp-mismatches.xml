<ruleset name="MailChimp (mismatches)" default_off="mismatch">

	<!--	herokuapp.com	-->
	<target host="status.mailchimp.com"/>

	<!--	status	-->
	<securecookie host="^(.*\.)?mailchimp\.com$" name=".*"/>


	<rule from="^http://status\.mailchimp\.com/"
		to="https://status.mailchimp.com/"/>

</ruleset>
