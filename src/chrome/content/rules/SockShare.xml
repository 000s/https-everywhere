	<target host="static.sockshare.com" />
	<target host="static1.sockshare.com" />
	<target host="www.sockshare.com" />


	<rule from="^http://(www\.)?sockshare\.com/(crossdomain\.xml|gopro\.php|include/captcha\.php)"
		to="https://$1sockshare.com/$2" />

