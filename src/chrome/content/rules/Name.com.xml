<ruleset name="Name.com (partial)">

	<target host="name.com"/>
	<target host="*.name.com"/>


	<securecookie host="^www\.name\.com$" name=".+" />


	<rule from="^http://(manage\.|www\.)?name\.com/"
		to="https://$1name.com/"/>

	<rule from="^http://support\.name\.com/(generated|system)/"
		to="https://assets.zendesk.com/$1/" />

</ruleset>
