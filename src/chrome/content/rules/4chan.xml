<ruleset name="4chan (partial)">

  <target host="4chan.org"/>
  <target host="*.4chan.org" />
  <target host="*.thumbs.4chan.org" />

  <securecookie host=".*\.4chan\.org$" name=".*"/>

  <rule from="^http://((boards|images|rs|static|(\d\.)?thumbs|www)\.)?4chan\.org/"
          to="https://$14chan.org/" />

</ruleset>
