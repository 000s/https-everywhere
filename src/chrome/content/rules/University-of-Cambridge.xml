<!--
	Lots of domains lack (valid) https, e.g.:

		- www.cam.ac.uk
		- www.lib.cam.ac.uk 
		- www.mrc-lmb.cam.ac.uk	(self-signed, expired; https breaks styling)
		- www.sports.cam.ac.uk
		- www.zoo		(times out)


	Problematic subdomains:

		- damtp		(cert only matches www.damtp)


	Partially covered subdomains:

		- www.damtp	(^ â†’ www, some pages 404)


	Fully covered subdomains:

		- webservices.admin
		- www.admin
		- www.cl
		- www2.mrc-lmb
		- www.wolfson

-->
<ruleset name="University of Cambridge (partial)">

	<target host="*.cam.ac.uk" />


	<rule from="^http://w(ebservices\.admin|ww2\.mrc-lmb|ww\.(?:admin|cl|wolfson))\.cam\.ac\.uk/"
		to="https://w$1.cam.ac.uk/" />

	<rule from="^http://(?:www\.)?damtp\.cam\.ac\.uk/global/"
		to="https://www.damtp.cam.ac.uk/global/" />

</ruleset>
