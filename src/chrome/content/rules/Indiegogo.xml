<ruleset name="Indiegogo (partial)">

	<target host="indiegogo.com"/>
	<target host="*.indiegogo.com"/>


	<!--	Tracking cookies:
					-->
	<securecookie host="^\.indiegogo\.com$" name="^(?:optimizely\w+|__utm\w)$" />


	<rule from="^http://(www\.)?indiegogo\.com/(account/|assets/|blog/|images/|styles/)"
		to="https://$1indiegogo.com/$2"/>

	<rule from="^http://web(\d)\.indiegogo\.com/"
		to="https://web$1.indiegogo.com/"/>

</ruleset>
