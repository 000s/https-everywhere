<!--	Bucket: d2oadd98wnjs7n.cloudfront.net


	Fully covered subdomains:

		- images

-->
<ruleset name="Indiegogo (partial)">

	<target host="indiegogo.com"/>
	<target host="*.indiegogo.com"/>


	<!--	Tracking cookies:
					-->
	<securecookie host="^\.indiegogo\.com$" name="^(?:optimizely\w+|__utm\w)$" />


	<rule from="^http://(www\.)?indiegogo\.com/(account/|assets/|blog/|images/|project/\d+/widget/|styles/)"
		to="https://$1indiegogo.com/$2"/>

	<rule from="^http://(images|web\d)\.indiegogo\.com/"
		to="https://$1.indiegogo.com/"/>

</ruleset>
