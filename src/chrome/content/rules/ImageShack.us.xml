<!--	See ImageShack.us-mismatches.xml also
-->
<ruleset name="ImageShack (partial)">

	<target host="imageshack.us"/>
	<target host=".imageshack.us"/>
	<target host="crackle.imageshack.us"/>
	<target host="desmond.imageshack.us"/>
	<target host="my.imageshack.us"/>
	<target host="register.imageshack.us"/>
	<target host="stream.imageshack.us"/>
	<target host="www.imageshack.us"/>

	<rule from="^http://(www\.)?([c-r]\w+\.)?imageshack\.us/" to="https://$2imageshack.us/"/>

	<rule from="^http://stream\.imageshack\.us/favicon\.ico$" to="https://imageshack.us/favicon.ico"/>

	<securecookie host="^\.?imageshack\.us$" name=".*"/>
</ruleset>
