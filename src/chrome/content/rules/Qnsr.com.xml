<!--
	For other QuinStreet coverage, see QuinStreet.xml.


	CDN buckets:

		- o1.qnsr.com.edgesuite.net/...

			- e1.cdn.qnsr.com


	Problematic domains:

		- e1.cdn.qnsr.com	(akamai)

-->
<ruleset name="Qnsr.com">

	<target host="*.qnsr.com" />


	<rule from="^http://(?:e1\.cdn|o1)\.qnsr\.com/"
		to="https://o1.qnsr.com/" />

</ruleset>