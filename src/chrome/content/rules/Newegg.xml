<!--
	CDN buckets:

		- images10.newegg.com.edgesuite.net	(maps directly)
			- a200.g.akamai.net/.../

		- images17.newegg.com.edgesuite.net/.../
			- a1268.g.akamai.net/.../

-->
<ruleset name="Newegg (partial)" default_off="mismatch">

	<target host="*.newegg.com" />
	<target host="c1.neweggimages.com" />


	<!--	- Akamai
		- Not identical
					-->
	<rule from="^http://images1([07])\.newegg\.com/"
		to="https://images1$1.newegg.com/" />

	<rule from="^https?://c1\.neweggimages\.com/"
		to="https://images10.neweggimages.com/" />

</ruleset>
