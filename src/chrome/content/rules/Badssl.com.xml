<!--
    Not secured subdomains:
        - All subdomains are not secured, because this can influence tests and cause unexpected results.
-->
<ruleset name="Badssl.com">
    <target host="badssl.com" />
    <!-- see https://github.com/lgarron/badssl.com/issues/80 -->
    <target host="httpseverywhere.badssl.com" />

    <rule from="^http:"
            to="https:" />
</ruleset>
