<!--
	Nonfunctional domains:

		- dl		(ssl_error_rx_record_too_long)
		- forum		(cert: triblr.org; redirects to www)

-->
<ruleset name="Triblr (partial)">

	<target host="triblr.org" />
	<target host="www.triblr.org" />


	<securecookie host="^www\.triblr\.org$" name=".*" />


	<rule from="^http://(www\.)?triblr\.org/"
		to="https://$1triblr.org/" />

</ruleset>
