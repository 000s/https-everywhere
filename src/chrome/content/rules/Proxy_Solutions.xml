<!--
	Some pages redirect to http.

-->
<ruleset name="Proxy Solutions (partial)">

	<target host="proxysolutions.net" />
	<target host="www.proxysolutions.net" />


	<rule from="^http://(www\.)?proxysolutions\.net/((?:affiliates|proxy-services|support|your-proxy-account)(?:$|\?|/)|favicon\.ico|images/|wp-content/|wp-includes/)"
		to="https://$1proxysolutions.net/$2" />

</ruleset>
