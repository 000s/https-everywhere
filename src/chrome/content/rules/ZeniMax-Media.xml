-->
<ruleset name="ZeniMax Media (partial)">

	<target host="bethsoft.com"/>
	<target host="*.bethsoft.com"/>
		<exclusion pattern="^http://(cdnstatic|support)\."/>

	<securecookie host="^(.*\.)?bethsoft\.com$" name=".*"/>

	<rule from="^http://(\w+\.)?bethsoft\.com/"
		to="https://$1bethsoft.com/"/>

</ruleset>
