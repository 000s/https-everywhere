	<rule from="^http://((asia|[bfk]r|eu|jp|latam)\.evga\.com|(www\.)?evga\.com\.(au|tw))/((.+/)?images/common/|favicon\.ico$)"
		to="https://www.evga.com/$2"/>

	<securecookie host="^www\.evga\.com$" name="dbforums.*"/>

