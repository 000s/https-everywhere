<ruleset name="Getsatisfaction.com">

	<target host="getsatisfaction.com"/>
	<target host="*.getsatisfaction.com"/>

	<rule from="^http://(www\.)?getsatisfaction\.com/"
		to="https://www.getsatisfaction.com/"/>

	<rule from="^http://assets(\d)\.getsatisfaction\.com/"
		to="https://assets$1.getsatisfaction.com/"/>

	<securecookie host="^\.getsatisfaction\.com$" name=".*"/>

</ruleset>
