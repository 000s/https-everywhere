<ruleset name="LinuxFoundation">
  <target host="linuxfoundation.org" />
  <target host="*.linuxfoundation.org" />

  <rule from="^http://(www\.)?linuxfoundation\.org/"
        to="https://www.linuxfoundation.org/"/>
  <rule from="^http://(events|ldn|training)\.linuxfoundation\.org/"
	to="https://$1.linuxfoundation.org/"/>
</ruleset>