<ruleset name="Legtux (cacert)" platform="cacert">
  <target host="legtux.org" />
  <target host="www.legtux.org" />


	<securecookie host="^www\.legtux\.org$" name=".+" />


  <rule from="^http://(?:www\.)?legtux\.org/" to="https://www.legtux.org/"/>
</ruleset>
