<ruleset name="ARPNetworks.com (mismatches)" default_off="Tender certificate">

	<target host="support.arpnetworks.com"/>

	<rule from="^http://support\.arpnetworks\.com/"
		to="https://support.arpnetworks.com/"/>

	<securecookie host="^support\.arpnetworks\.com$" name=".*"/>

</ruleset>
