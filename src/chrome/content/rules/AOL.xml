<ruleset name="AOL (partial)">
	<target host="bill.aol.com" />
	<target host="contactus.aol.com" />
	<target host="account.login.aol.com" />
	<target host="api.mail.aol.com" />
	<target host="myaccount.aol.com" />
	<target host="new.aol.com" />
	<target host="openid.aol.com" />
	<target host="*.oscar.aol.com"/>
	<target host="pki-info.aol.com" />
	<target host="aolctoftp.red.aol.com" />
	<target host="s2c.aol.com" />
	<target host="api.screenname.aol.com" />
	<target host="my.screenname.aol.com" />
	<target host="cdn.webmail.aol.com" />
	<target host="dashboard.voice.aol.com" />
	<target host="o.aolcdn.com" />
	<target host="s.aolcdn.com" />
	<target host="sns-static.aolcdn.com" />

	<rule from="^http://(bill|contactus|(account\.login)|(api\.mail)|myaccount|new|openid|pki-info|(aolctoftp\.red)|s2c|((api|my)\.screenname)|(dashboard\.voice)|cdn\.webmail)\.aol\.com/" to="https://$1.aol.com/" />

	<rule from="^http://(?:exp)?api\.oscar\.aol\.com/"
		to="https://api.oscar.aol.com/"/>

	<rule from="^http://o\.aolcdn\.com/"
		to="https://s.aolcdn.com/" />

	<rule from="^http://(s|sns-static)\.aolcdn\.com/"
		to="https://$1.aolcdn.com/" />
</ruleset>
