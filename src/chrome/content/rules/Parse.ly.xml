<!--
	For problematic rules, see Parse.ly-mismatches.xml.


	Nonfunctional domains:

		- (www.)parse.ly
		- blog.parse.ly		(hosted on Tumblr)
		- (www.)parsely.com
		- blog.parsely.com	(redirects to .ly)

-->
<ruleset name="Parse.ly (partial)">

	<target host="*.parsely.com" />


	<rule from="^http://(api|dash)\.parsely\.com/"
		to="https://$1.parsely.com/" />

</ruleset>
