    <securecookie host="^(.*\.)?getsafeonline\.org$" name=".*" />

    <rule from="^https?://getsafeonline\.com/"
        to="https://www.getsafeonline.com/" />
    <rule from="^http://([^/:@]+)\.getsafeonline\.com/"
        to="https://$1.getsafeonline.com/" />
</ruleset>
