<!--
	Nonfunctional:

		- (www.)

-->
<ruleset name="Mookie1.com">
	<!--	* for cross-domain cookie	-->
	<target host="*.mookie1.com" />
	<target host="t.mookie1.com" />
	<target host="b3-uk.mookie1.com" />
	<target host="b3.mookie1.com" />
	<target host="dna1.mookie1.com" />


	<securecookie host="^\.mookie1\.com$" name=".*" />


	<rule from="^http://(t|b3-uk|b3|dna1)\.mookie1\.com/" to="https://$1.mookie1.com/" />

	<!--	Tracking beacon.	-->
	<rule from="^http://t1\.mookie1\.com/"
		to="https://t1.mookie1.com/" />

</ruleset>