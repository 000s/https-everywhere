<!--
	This ruleset is for Marketo clients that
	have no other rules off by default.

-->
<ruleset name="Marketo clients" default_off="mismatch">

	<!--	cert: *.marketo.com	-->
	<target host="go.proofpoint.com" />


	<!--	These paths 404:

			- formstandard-2column.html

				-->
	<rule from="^http://go\.proofpoint\.com/(css|images)/"
		to="https://go.proofpoint.com/$1/" />

</ruleset>
