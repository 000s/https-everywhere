<ruleset name="DreamHost (partial)">

	<target host="dreamhost.com" />
	<target host="*.dreamhost.com" />


	<securecookie host="^(\.)?(?:discussion|files|gifts|media|panel|signup|webftp)\.dreamhost\.com$" name=".+" />


	<rule from="^http://((?:discussion|files|gifts|media|panel|signup|webftp|www)\.)?dreamhost\.com/"
		to="https://$1dreamhost.com/" />

</ruleset>