<ruleset name="Elsevier (mismatches)" default_off="mismatches">

	<target host="cdn.els-cdn.com" />
	<target host="elsevier.com"/>
	<target host="www.elsevier.com"/>


	<rule from="^http://cdn\.els-cdn\.com/"
		to="https://cdn.els-cdn.com/" />

	<rule from="^http://(?:www\.)?elsevier\.com/(authored_framework/|favicon\.ico$|framework_(hom|includ)e/)"
		to="https://www.elsevier.com/$1"/>

</ruleset>
