<ruleset name="Open Source Robotics Foundation (partial)">

	<target host="osrfoundation.org" />
	<target host="www.osrfoundation.org" />
		<!--
			Redirects to http:
						-->
		<!--exclusion pattern="^http://www\.osrfoundation\.org/+($|modx/assets/|support-osrf\.html)" /-->
		<!--
			Exceptions:
					-->
		<exclusion pattern="^http://www\.osrfoundation\.org/+(?!favicon\.ico|wordpress/wp-content/)" />


	<rule from="^http://(www\.)?osrfoundation\.org/"
		to="https://$1osrfoundation.org/" />

</ruleset>
