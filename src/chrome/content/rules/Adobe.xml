	<rule from="^http://login\.hitbox\.com/"
		to="https://login.hitbox.com/"/>

	<rule from="^http://fpdownload\.macromedia\.com/"
		to="https://fpdownload.macromedia.com/"/>

	<rule from="^http://omniture\.com/"
