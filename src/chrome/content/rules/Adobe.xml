	<rule from="^http://(\w+)\.tt\.omtrdc\.net/"
		to="https://$1.tt.omtrdc.net/"/>

	<rule from="^http://(?:www1?\.)?scene7\.com/"
		to="https://www1.scene7.com/"/>

	<rule from="^http://use\.typekit\.com/"
