<!--
	Nonfunctional subdomains:

		- s(1[25]|2[0129]|4[47]|51|m[59]).sitemeter.com

-->
<ruleset name="sitemeter.com">

	<target host="*.sitemeter.com" />


	<rule from="^http://www\.sitemeter\.com/"
		to="https://www.sitemeter.com/" />

	<rule from="^https?://\w\w\.sitemeter\.com/js/"
		to="https://www.sitemeter.com/js/" />

</ruleset>
