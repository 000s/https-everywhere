<ruleset name="9gag">
  <target host="9gag.com"/>
  <target host="www.9gag.com"/>
  <target host="m.9gag.com"/>
  
  <securecookie host="^(.*\.)?9gag\.com$" name=".*" />
  
  <rule from="^http://(?:www\.)?9gag\.com/" to="https://9gag.com/"/>
  <rule from="^http://m\.9gag\.com/" to="https://m.9gag.com/"/>
</ruleset>
