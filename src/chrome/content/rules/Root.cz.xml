<ruleset name="Root.cz">
	<target host="root.cz" />
	<target host="www.root.cz" />
	<target host="forum.root.cz" />
	<target host="blog.root.cz" />
	<target host="rss.root.cz" />
<!--
  Broken subdomains:
  butik.root.cz
  partner.root.cz
-->

	<securecookie host=".+\.root\.cz$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
