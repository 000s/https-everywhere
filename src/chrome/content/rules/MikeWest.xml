<!-- Partial ruleset. No https for static.mikewest.org -->
<ruleset name="MikeWest">
  <target host="mikewest.org" />
  <target host="www.mikewest.org" />

  <securecookie host="^(.+\.)?mikewest\.org$" name=".*"/>

  <rule from="^http://(?:www\.)?mikewest\.org/" to="https://mikewest.org/"/>
</ruleset>
