<!--
	Problematic subdomains:

		- (www.) *

	* Works, cert only matches secure


	Fully covered subdomains:

		- (www.)	(â†’ secure)
		- secure

-->
<ruleset name="DD-WRT">

	<target host="dd-wrt.com" />
	<target host="*.dd-wrt.com" />


	<securecookie host=".*\.dd-wrt\.com$" name=".+" />


	<rule from="^http://(?:secure\.|www\.)?dd-wrt\.com/"
		to="https://secure.dd-wrt.com/" />

</ruleset>
