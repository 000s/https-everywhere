<!--
	Nonfunctional subdomain:
		- $     -> domain mismatch
-->
<ruleset name="Hft-leipzig.de">
	<target host="hft-leipzig.de" />
	<target host="www.hft-leipzig.de" />

	<securecookie host="." name="." />

	<rule from="^http://hft-leipzig\.de/"
		to="https://www.hft-leipzig.de/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
