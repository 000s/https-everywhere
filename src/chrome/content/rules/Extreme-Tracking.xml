<!--
	Other eXTReMe Tracking rulesets:

		 Extreme-Dm.com.xml

-->
<ruleset name="Extreme Tracking">

	<target host="extremetracking.com" />
	<target host="www.extremetracking.com" />


	<!--	Cert only matches ^extremetracking.com.
							-->
	<rule from="^https?://(?:www\.)?extremetracking\.com/"
		to="https://extremetracking.com/" />

</ruleset>
