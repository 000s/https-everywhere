<ruleset name="Nodejitsu.com (partial)">

	<target host="nodejitsu.com" />
	<target host="*.nodejitsu.com" />


	<rule from="^http://(www\.)?nodejitsu\.com/"
		to="https://$1nodejitsu.com/" />

	<rule from="^http://(?:www\.)?(blog|legal)\.nodejitsu\.com/"
		to="https://$1.nodejitsu.com/" />

</ruleset>
