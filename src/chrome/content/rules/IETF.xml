<!--
	Fully covered subdomains:

		- (www.)
		- iaoc
		- datatracker
		- mailarchive
		- tools
		- wiki.tools
		- trustee


	Mixed content:

		- Images on www from $self *

	* Secured by us

-->
<ruleset name="IETF">
  <target host="ietf.org" />
  <target host="*.ietf.org" />


	<securecookie host="^\.ietf\.org$" name="^__cfduid$" />


  <rule from="^http://(?:www\.)?ietf\.org/" to="https://www.ietf.org/"/>
  <rule from="^http://(datatracker|iaoc|mailarchive|tools|wiki\.tools|trustee)\.ietf\.org/" to="https://$1.ietf.org/"/>
</ruleset>
