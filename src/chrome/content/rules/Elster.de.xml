<ruleset name="Elster.de">
	<target host="www.elster.de" />
	<target host=    "elster.de" />
	<target host="www.elsteronline.de" />
	<target host=    "elsteronline.de" />

	<rule from="^https?://elster\.de/"
		to="https://www.elster.de/" />
	<rule from="^https?://elsteronline\.de/"
		to="https://www.elsteronline.de/" />
	<rule from="^http:"
	        to="https:" />
</ruleset>
