<ruleset name="Pinterest (mismatches)" default_off="Akamai certificate">

	<target host="media-cache.pinterest.com"/>
	<target host="media-cache0.pinterest.com"/>
	<target host="media-cache1.pinterest.com"/>
	<target host="media-cache2.pinterest.com"/>
	<target host="media-cache3.pinterest.com"/>
	<target host="media-cache4.pinterest.com"/>
	<target host="media-cache5.pinterest.com"/>
	<target host="media-cache6.pinterest.com"/>
	<target host="media-cache7.pinterest.com"/>
	<target host="media-cache8.pinterest.com"/>
	<target host="media-cache9.pinterest.com"/>
	<target host="passets-cdn.pinterest.com"/>

	<rule from="^http://media-cache[0-9]?\.pinterest\.com/"
		to="https://media-cache.pinterest.com/"/>

	<rule from="^http://passets-cdn\.pinterest.com/"
		to="https://passets-cdn\.pinterest.com/"/>

</ruleset>
