<ruleset name="Kintera Network">
	<target host="kintera.org" />
	<target host="*.kintera.org" />
	<target host="www.*.kintera.org" />
	<target host="kintera.com" />
	<target host="www.kintera.com" />

	<rule from="^http://(www\.)?kintera\.org/" to="https://www.kintera.org/" />
	<rule from="^http://(?:[\w-]+\.){1,2}kintera\.org/(?=[^/]+/[^/])" to="https://www.kintera.org/" />
	<rule from="^http://(www\.)?kintera\.com/" to="https://www.kintera.com/" />
</ruleset>
