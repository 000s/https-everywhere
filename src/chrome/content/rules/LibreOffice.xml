<!--
	Nonfunctional subdomains:

		- ask *
		- dev-builds	(times out)

	* Times out

-->
<ruleset name="LibreOffice">

	<target host="libreoffice.org" />
	<target host="*.libreoffice.org" />


	<securecookie host="^(?:help|www)\.libreoffice\.org$" name=".*" />


	<rule from="^http://(?:www\.)?libreoffice\.org/"
		to="https://www.libreoffice.org/" />

	<rule from="^http://help\.libreoffice\.org/"
		to="https://help.libreoffice.org/" />

</ruleset>
