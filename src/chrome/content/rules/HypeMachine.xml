<ruleset name="HypeMachine">
    <target host="hypem.com" />
    <target host="*.hypem.com" />
    <securecookie host="(([^/:@]*)\.)*hypem\.com/" name=".*" />
    <rule from="^http://hypem\.com/" to="https://hypem.com/" />
    <rule from="^http://([^/:@]*)\.hypem\.com/" to="https://$1.hypem.com/" />
</ruleset>
