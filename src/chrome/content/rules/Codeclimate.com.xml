<!--
	Nonfunctional subdomains:
		- docs   -> wrong domain
		- blog   -> wrong domain
		- status   -> wrong domain
-->
<ruleset name="Codeclimate.com">
    <target host="codeclimate.org" />
    <target host="www.codeclimate.org" />

    <securecookie host="^(www\.)?codeclimate\.com" name=".+" />

    <rule from="^http:"
            to="https:" />
</ruleset>
