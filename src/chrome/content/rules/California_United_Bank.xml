<ruleset name="California United Bank">

	<target host="californiaunitedbank.com" />
	<target host="*.californiaunitedbank.com" />
	<target host="cunb.com" />
	<target host="*.cunb.com" />


	<securecookie host="^(?:w*\.)?c(?:aliforniaunitedbank|unb)\.com$" name=".+" />


	<rule from="^http://(www\.)?c(aliforniaunitedbank|unb)\.com/"
		to="https://$1c$2.com/" />

</ruleset>