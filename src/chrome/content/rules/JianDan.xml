<!--

	mismatch:
		- g
		- s
		
		But equal to ^ :
		- cdn
		- www
		
	MCB:
	  - ^

-->

<ruleset name="JianDan (partial)">
	
	<target host="cdn.jiandan.net" />
	<rule from="^http://cdn\.jiandan\.net/" to="https://jiandan.net/" />
		<test url="http://cdn.jandan.net/static/img/favicon.ico" />
	
	<target host="jiandan.net" />
	<target host="www.jiandan.net" />
	  <exclusion pattern="^http://(www\.)?jiandan\.net/(?![\w/-]+\.(gif|png))" />
	  
	  <test url="http://jandan.net/ofk.gif" />
	  <test url="http://jandan.net/share/2016/06/qr-79613.png" />
	  <test url="http://www.jandan.net/ofk.gif" />
	  <test url="http://www.jandan.net/share/2016/06/qr-79613.png" />
	  
	<target host="tankr.net" />

	<rule from="^http:" to="https:" />

</ruleset>
