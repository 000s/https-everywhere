<ruleset name="AOL CDN" default_off="Akamai certificate">

	<target host="aolcdn.com" />
	<target host="o.aolcdn.com" />
	<target host="www.aolcdn.com" />

	<rule from="^http://(www\.)?aolcdn\.com/"
		to="https://www.aolcdn.com/" />

	<rule from="^http://o\.aolcdn\.com/"
		to="https://o.aolcdn.com/" />

</ruleset>
