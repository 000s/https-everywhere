<ruleset name="Vagrantup.com (problematic)" default_off="mismatched">

	<target host="vagrantup.com" />
	<target host="www.vagrantup.com" />


	<rule from="^http://(?:www\.)?vagrantup\.com/"
		to="https://www.vagrantup.com/" />

</ruleset>