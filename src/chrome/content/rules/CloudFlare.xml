<ruleset name="CloudFlare">

	<target host="cloudflare.com"/>
	<target host="*.cloudflare.com"/>
	<target host=".www.cloudflare.com"/>

	<securecookie host="^.*\.?cloudflare\.com$" name=".*"/>

	<rule from="^http://((ajax|cdnjs|support|www)\.)?cloudflare\.com/"
		to="https://$1cloudflare.com/"/>

</ruleset>
