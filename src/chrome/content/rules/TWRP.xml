<!--
	Non-functional domain:
		- teamw.in
-->
<ruleset name="TWRP">

	<target host=    "twrp.me" />
	<target host="www.twrp.me" />
	<target host= "dl.twrp.me" />
	<target host=    "teamw.in" />
	<target host="www.teamw.in" />
	<target host= "dl.teamw.in" />

  	<securecookie host="^.*\.twrp\.me$" name=".+" />

	<rule from="^http://(www\.|dl\.)?teamw\.in/"
		to="https://$1twrp.me/" />

	<rule from="^http:" 
		to="https:" />

</ruleset>
