<!--
	Mismatch:
		- *.cdn-surfline.com
	
	Timeout:
		- cdn02

	Refused:
		- media1
		- premium

HTTP redircet on articles, mixedcontent on home
-->
<ruleset name="Surfline" platform="mixedcontent redirect">
	<target host="surfline.com" />
	<target host="www.surfline.com" />

	<rule from="^http://(?:www\.)?surfline\.com/"
		to="https://www.surfline.com/" />
</ruleset>
