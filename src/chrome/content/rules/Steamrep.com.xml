<ruleset name="Steamrep.com">
  <target host="steamrep.com" />
  <target host="www.steamrep.com" />
  <exclusion pattern="^http://(www\.)?forums.steamrep.com/" />
          <test url="http://www.forums.steamrep.com/" />
          <test url="http://forums.steamrep.com/" />
  <rule from="^http:" to="https:" />
</ruleset>
