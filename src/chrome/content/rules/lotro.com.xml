<ruleset name="lotro.com">
	<target host="www.lotro.com" />
	<target host="lotro.com" />
	<target host="store.lotro.com" />
	<target host="signup.lotro.com" />
	<target host="transfers.lotro.com" />

	<rule from="^http://(www\.)?lotro\.com/" to="https://www.lotro.com/"/>
	<rule from="^http://store\.lotro\.com/" to="https://store.lotro.com/"/>
	<rule from="^http://signup\.lotro\.com/" to="https://signup.lotro.com/"/>
	<rule from="^http://transfers\.lotro\.com/" to="https://transfers.lotro.com/"/>
</ruleset>
