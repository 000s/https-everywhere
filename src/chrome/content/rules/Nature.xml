<ruleset name="Nature (partial)">

	<target host="*.nature.com" />


	<securecookie host="^secure\.nature\.com$" name=".*" />


	<rule from="^http://s(ecure|tatus)\.nature\.com/"
		to="https://s$1.nature.com/" />

</ruleset>
