<!--
	Bucket at s3.amazonaws.com/pbase/


	Nonfunctional subdomains:

		- (www.)
		- ct4		(cert: secure.pbase.com, expired; redirects to secure2)
		- i		(times out)
		- ic2
		- thump01

-->
<ruleset name="PBase (partial)">

	<target host="pbase.com" />
	<target host="*.pbase.com" />


	<!--	ap1: cloudfront; bucket unknown.
							-->
	<rule from="^https?://(?:ap1\.|secure2\.|www\.)?pbase\.com/"
		to="https://secure2.pbase.com/" />

</ruleset>
