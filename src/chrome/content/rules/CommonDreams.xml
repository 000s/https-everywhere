<!--
	Nonfunctional subdomains:

		- www *

	* Redirects to http


	Problematic subdomains:

		- action *

	* Mismatched

-->
<ruleset name="CommonDreams (partial)">
  <target host="commondreams.org" />
  <target host="www.commondreams.org" />
		<!--
			Started redirects to http:
							-->
		<!--exclusion pattern="^http://www\.commondreams\.org/+($|\?|donate($|\?)|news/\d{4}/\d\d/\d\d/[\w-]+/|sites/)" /-->

  <securecookie host="^(?:www\.)?commondreams\.org$" name=".*"/>

  <rule from="^http://(?:www\.)?commondreams\.org/" to="https://www.commondreams.org/"/>
</ruleset>
