<ruleset name="Furaffinity">

	<target host="d.facdn.net" />
	<target host="furaffinity.net" />
	<target host="*.furaffinity.net" />


	<securecookie host="^(?:ox|sfw|www)?\.furaffinity\.net$" name=".+" />


	<rule from="^http://d\.facdn\.net/"
		to="https://d.facdn.net/" />

	<rule from="^http://((?:ox|sfw|www)\.)?furaffinity\.net/"
		to="https://$1furaffinity.net/" />

</ruleset>
