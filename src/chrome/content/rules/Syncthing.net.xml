
<!--

	Fully covered hosts in *syncthing.net:

		- (www.)?
		- forum
		- docs

-->
<ruleset name="Syncthing.net">

	<!--	Direct rewrites:
				-->
	<target host="syncthing.net" />
	<target host="forum.syncthing.net" />
	<target host="www.syncthing.net" />
	<target host="docs.syncthing.net" />


	<rule from="^http:"
		to="https:" />

</ruleset>
