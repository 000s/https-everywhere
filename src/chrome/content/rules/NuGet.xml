<ruleset name="NuGet">
  <target host="nuget.org" />
  <target host="*.nuget.org" />

  <!-- https://trac.torproject.org/projects/tor/ticket/7138 -->
  <exclusion pattern="^http://docs\.nuget\.org/" />

  <rule from="^http://nuget\.org/" to="https://nuget.org/"/>
  <rule from="^http://([^/:@]*)\.nuget\.org/" to="https://$1.nuget.org/"/>
</ruleset>
