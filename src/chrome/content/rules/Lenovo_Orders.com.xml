<!--
	For other Lenovo coverage, see Lenovo.xml.


	(www.)?: Presents no certificate chain


	Fully covered domains:

		- (www.)lenovoorders.com

-->
<ruleset name="Lenovo Orders.com" default_off='missing certificate chain'>

	<target host="lenovoorders.com" />
	<target host="www.lenovoorders.com" />


	<rule from="^http://(www\.)?lenovoorders\.com/"
		to="https://$1lenovoorders.com/" />

</ruleset>
