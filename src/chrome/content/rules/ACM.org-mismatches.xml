<!--
	For rules that are on by default, see ACM.org.xml.

-->
<ruleset name="ACM.org (mismatches)" default_off="mismatch">

	<!--	Akamai	-->
	<target host="deliveryimages.acm.org" />
	<target host="portalparts.acm.org" />


	<rule from="^http://(deliveryimage|portalpart)s\.acm\.org/"
		to="https://$1s.acm.org/" />

</ruleset>
