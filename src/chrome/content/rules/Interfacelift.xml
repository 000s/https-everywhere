<ruleset name="Interfacelift">
    <target host="interfacelift.com" />
    <target host="www.interfacelift.com" />

    <securecookie host="^(.*\.)?interfacelift\.com$" name=".*" />

    <rule from="^http://interfacelift\.com/"
        to="https://interfacelift.com/" />
    <rule from="^http://([^/:@]+)\.interfacelift\.com/"
        to="https://interfacelift.com/" />
</ruleset>
