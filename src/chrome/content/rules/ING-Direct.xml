	<target host="ingdirect.com.au" />
	<target host="www.ingdirect.com.au" />


	<securecookie host="^www\.ingdirect\.com\.au$" name=".*" />


	<!--	Cert only matches www.	-->
	<rule from="^https?://(?:www\.)?ingdirect\.com\.au/"
		to="https://www.ingdirect.com.au/" />

</ruleset>
