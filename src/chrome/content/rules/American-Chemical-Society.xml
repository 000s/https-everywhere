<!--
	Other American Chemical Society rulesets:

		- Chemical_Abstracts_Service.xml

-->
<ruleset name="American Chemical Society (partial)">

	<target host="*.acs.org"/>


	<securecookie host="^assets\.acs\.org$" name=".*"/>


	<rule from="^http://(assets|portal)\.acs\.org/"
		to="https://$1.acs.org/"/>

	<!--	most pages redirect to http	-->
	<rule from="^http://pubs\.acs\.org/(appl/|action/showLogin|sda/|templates/)"
		to="https://pubs.acs.org/$1"/>

</ruleset>
