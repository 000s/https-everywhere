<ruleset name="OpenX (partial)">

	<target host="i-cdn.openx.com" />
	<target host="*.openx.org" />


	<securecookie host="^i-cdn\.openx\.com$" name=".+" />
	<securecookie host="^(d1|developer)\.openx\.org$" name=".+" />


	<rule from="^http://i-cdn\.openx\.com/"
		to="https://i-cdn.openx.com/" />

	<rule from="^http://d(1|eveloper)\.openx\.org/"
		to="https://d$1.openx.org/" />

</ruleset>
