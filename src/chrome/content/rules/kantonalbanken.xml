<ruleset name="Schweizer Kantonalbanken">
	<target host="www.blkb.ch"/>
	<target host="www.bcvs.ch"/>
	<target host="*.bcvs.ch"/>
	<target host="www.bcv.ch"/>
	<target host="*.bcv.ch"/>
	<target host="www.bcge.ch"/>
	<target host="*.bcge.ch"/>
	<target host="www.bcj.ch"/>
	<target host="*.bcj.ch"/>
	<target host="www.tkb.ch"/>
	<target host="*.tkb.ch"/>
	<target host="www.glkb.ch"/>
	<target host="*.glkb.ch"/>
	<target host="www.akb.ch"/>
	<target host="*.akb.ch"/>
	<target host="www.shkb.ch"/>
	<target host="*.shkb.ch"/>
	<target host="www.bcf.ch"/>
	<target host="*.bcf.ch"/>
	<target host="www.bancastato.ch"/>
	<target host="*.bancastato.ch"/>
	<target host="www.zugerkb.ch"/>
	<target host="*.zugerkb.ch"/>
	<target host="www.zkb.ch"/>
	<target host="*.zkb.ch"/>
	<target host="www.yourmoney.ch"/>
	<target host="*.yourmoney.ch"/>
	<exclusion pattern="ebanking\.blkb\.ch/"/>
	
	<rule from="http://www.blkb.ch/"
			to="https://secure.blkb.ch/"/>
	<rule from="^http://([^/:@]*)\.bcvs\.ch/"
		to="https://$1.bcvs.ch/"/>
	<rule from="^http://([^/:@]*)\.bcv\.ch/"
		to="https://$1.bvc.ch/"/>
	<rule from="^http://([^/:@]*)\.bcge\.ch/"
		to="https://$1.bcge.ch/"/>
	<rule from="^http://([^/:@]*)\.bcj\.ch/"
		to="https://$1.bcj.ch/"/>
	<rule from="^http://([^/:@]*)\.tkb\.ch/"
		to="https://$1.tkb.ch/"/>
	<rule from="^http://([^/:@]*)\.glkb\.ch/"
		to="https://$1.glkb.ch/"/>
	<rule from="^http://([^/:@]*)\.akb\.ch/"
		to="https://$1.akb.ch/"/>
	<rule from="^http://([^/:@]*)\.shkb\.ch/"
		to="https://$1.shkb.ch/"/>
	<rule from="^http://([^/:@]*)\.bcf\.ch/"
		to="https://$1.bcf.ch/"/>
	<rule from="^http://([^/:@]*)\.bancastato\.ch/"
		to="https://$1.bancastato.ch/"/>
	<rule from="^http://([^/:@]*)\.zugerkb\.ch/"
		to="https://$1.zugerkb.ch/"/>
	<rule from="^http://([^/:@]*)\.yourmoney\.ch/"
		to="https://$1.zkb.ch/"/>
	<rule from="^http://([^/:@]*)\.yourmoney\.ch/"
		to="https://$1.zkb.ch/"/>
</ruleset>
