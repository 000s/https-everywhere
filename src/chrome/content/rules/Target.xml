<!--
	CDN buckets:

		- www.target.com.edgesuite.net
		- img1.targetimg1.com.edgesuite.net


	Nonfunctional target.com subdomains:

		- ^			(times out)
		- www			("Oh no", akamai)


	Problematic domains:

		- img1.targetimg1.com	(works, akamai)

-->
<ruleset name="Target (partial)" default_off="mismatch">

	<target host="img1.targetimg1.com" />


	<rule from="^http://img1\.targetimg1\.com/"
		to="https://img1.targetimg1.com/" />

</ruleset>
