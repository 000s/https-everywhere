<ruleset name="ShopWiki (partial)" platform="mixedcontent">

	<target host="shopwiki.com" />
	<target host="*.shopwiki.com" />


	<securecookie host="^.*\.shopwiki\.com$" name=".+" />


	<rule from="^http://((?:static2|staticssl|wiki|www)\.)?shopwiki\.com/"
		to="https://$1shopwiki.com/" />

</ruleset>