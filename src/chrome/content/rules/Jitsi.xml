<ruleset name="Jitsi">

	<target host="jit.si" />
	<target host="www.jit.si" />
	<target host="jitsi.org" />
	<target host="www.jitsi.org" />


	<securecookie host="^jitsi\.org$" name=".*" />


	<rule from="^http://(www\.)?jit\.si/"
		to="https://$1jit.si/" />

	<rule from="^http://(www\.)?jitsi\.org/"
		to="https://$1jitsi.org/" />

</ruleset>
