<!--
	CDN buckets:

		- s3.amazonaws.com/dailybooth/

-->
<ruleset name="DailyBooth" default_off="mismatch">

	<!--	Cert: *.cloudfront.net
					-->
	<target host="dailybooth.com" />
	<target host="*.dailybooth.com" />


	<!--	All appear to be identical.
						-->
	<rule from="^http://(?:cdn[1-3]\.|cloudfront\.|www\.)?dailybooth\.com/"
		to="https://cloudfront.dailybooth.com/" />

</ruleset>
