<ruleset name="AddThis">
  <target host="*.addthis.com" />
  <target host="addthis.com" />

  <rule from="^http://(?:www\.)?addthis\.com/bookmark\.php"
	  to="https://www.addthis.com/bookmark.php"/>
  <rule from="^http://api\.addthis\.com/"
	  to="https://api.addthis.com/"/>
  <rule from="^http://s(3|5|7|9)\.addthis\.com/"
	  to="https://s$1.addthis.com/"/>
  <rule from="^http://ds\.addthis\.com/"
	  to="https://ds.addthis.com/" />
  <rule from="^http://cache\.addthis\.com/"
	  to="https://cache.addthis.com/" />
</ruleset>
