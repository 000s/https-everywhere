<ruleset name="Powells.com">
  <target host="powells.com" />
  <target host="*.powells.com" />


	<securecookie host="^\.powells\.com$" name="^(?:lastlocation|preferences|p_session|trail_id)$" />


  <rule from="^http://(?:www\.)?powells\.com/" to="https://www.powells.com/" />
  <rule from="^http://content-[0-9]\.powells\.com/" to="https://www.powells.com/" />
</ruleset>
