<!--
	For other Attachmate Group coverage, see Attachmate-Group.xml.


	Problematic subdomains:

		- ^	(refused)

-->
<ruleset name="NetIQ">

	<target host="netiq.com" />
	<target host="*.netiq.com" />


	<securecookie host="^(?:www)?\.netiq\.com$" name=".*" />


	<rule from="^http://(?:www\.)?netiq\.com/"
		to="https://www.netiq.com/"/>

</ruleset>