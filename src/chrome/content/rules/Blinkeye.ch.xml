<ruleset name="blinkeye.ch" default_off="self-signed">

	<target host="blinkeye.ch"/>
	<target host="*.blinkeye.ch"/>

	<rule from="^http://(?:www\.)?blinkeye\.ch/$"
		to="https://blinkeye.ch/dokuwiki/doku.php"/>

	<rule from="^http://(?:www\.)?blinkeye\.ch/(.+)"
		to="https://blinkeye.ch/$1"/>

	<securecookie host="^(www)?.blinkeye\.ch$" name=".*"/>

</ruleset>
