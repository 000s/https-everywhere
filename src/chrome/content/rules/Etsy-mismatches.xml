<!--
	For rules that are on by default, see Etsy.xml.

-->
<ruleset name="Etsy (mismatches)" default_off="mismatch">

	<!--	Akamai	-->
	<target host="*.etsystatic.com" />


	<!--	Data appear identical across img\d,
		so minimize number of exceptions needed.	-->
	<rule from="^https?://img\d\.etsystatic\.com/"
		to="https://img0.etsystatic.com/" />

</ruleset>
