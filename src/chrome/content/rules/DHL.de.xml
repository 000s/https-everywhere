<ruleset name="DHL.de" platform="mixedcontent">
  <target host="dhl.de"/>
  <target host="*.dhl.de"/>

  <securecookie host="^(?:.*\.)?dhl\.de$" name=".*" />

  <rule from="^http://(?:www\.)?dhl\.de/" to="https://www.dhl.de/"/>
  <rule from="^http://nolp\.dhl\.de/" to="https://nolp.dhl.de/"/>
</ruleset>
