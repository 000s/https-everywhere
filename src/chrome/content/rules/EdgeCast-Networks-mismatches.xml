<!--
	For rules that are on by default, see EdgeCast-networks.xml.

-->
<ruleset name="EdgeCast Networks (mismatches)" default_off="mismatch">

	<target host="*.biz.edgesuite.net" />
	<target host="*.com.edgesuite.net" />
	<target host="*.info.edgesuite.net" />
	<target host="*.net.edgesuite.net" />
	<target host="*.org.edgesuite.net" />


	<rule from="^http://(\w+\.\w+)\.edgesuite\.net/"
		to="https://$1.edgesuite.net/" />

</ruleset>
