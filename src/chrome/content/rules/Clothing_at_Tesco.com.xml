<ruleset name="Clothing at Tesco.com">

	<target host="clothingattesco.com" />
	<target host="*.clothingattesco.com" />


	<securecookie host="^(.*\.)?clothingattesco\.com$" name=".*" />


	<rule from="^http://(?:www\.)?clothingattesco\.com/"
		to="https://www.clothingattesco.com/" />


</ruleset>
