<!--
	Problematic domains:

		- www.linux.conf.au *

	* Mismatched, CN: lca2013.linux.org.au

-->
<ruleset name="linux.conf.au">

	<target host="linux.conf.au" />
	<target host="www.linux.conf.au" />
	<target host="lca2014.linux.org.au" />


	<rule from="^http://(?:www\.)?linux\.conf\.au/"
		to="https://linux.conf.au/" />

	<rule from="^http://lca2014\.linux\.org\.au/"
		to="https://lca2014.linux.org.au/" />

</ruleset>
