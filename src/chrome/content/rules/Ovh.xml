<ruleset name="OVH">

	<target host="ovh.com" />
	<target host="www.ovh.com" />
	<target host="ovh.co.uk" />
	<target host="www.ovh.co.uk" />
	<target host="ovh.de" />
	<target host="www.ovh.de" />


	<securecookie host="^(?:.*\.)?ovh\.(?:com|co\.uk|de)$" name=".+" />


	<rule from="^http://(?:www\.)?ovh\.(com|co\.uk|de)/"
		to="https://www.ovh.$1/" />

</ruleset>
