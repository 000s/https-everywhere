<!--
	For rules that are on by default, see Myspace.xml.

-->
<ruleset name="Myspace (mismatches)" default_off="mismatch">

	<!--	 cert: gp1.wac.edgecastcdn.net	-->
	<target host="*.ec-images.myspacecdn.com" />


	<!--	1-4 in a* appear identical, so minimize exceptions needed.
										-->
	<rule from="^https?://a[1-4]\.ec-images\.myspacecdn\.com/"
		to="https://a1.ec-images.myspacecdn.com/" />

</ruleset>
