<!--
	^steaw.com: Shows another domain

-->
<ruleset name="Steaw">

	<!--	Direct rewrites:
				-->
	<target host="www.steaw.com" />

	<!--	Complications:
				-->
	<target host="steaw.com" />


	<rule from="^http://steaw\.com/"
		to="https://www.steaw.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
