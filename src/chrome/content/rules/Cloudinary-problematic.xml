<ruleset name="Cloudinary (problematic)" default_off="mismatched">

	<target host="res.cloudinary.com" />
	<target host="*.res.cloudinary.com" />


	<rule from="^https?://(?:a\d\.)?res\.cloudinary\.com/"
		to="https://res.cloudinary.com/" />

</ruleset>