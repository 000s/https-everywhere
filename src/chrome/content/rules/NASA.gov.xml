<!--
	Other US government rulesets:

		- US-government.xml
		- US-government-mismatches.xml


	Nonfunctional:

		- www.cdscc.nasa.gov	(cert: TS Series NAS; redirects infinitely)
		- jpl.nasa.gov		(times out; redirects to www over http)
		- www.jps.nasa.gov
		- pds.nasa.gov		(timeout)
		- visibleearth.nasa.gov	(times out)

-->
<ruleset name="NASA">

	<target host="corts.hq.nasa.gov" />
	<target host="solarsystem.nasa.gov" />


	<securecookie host="^(corts\.hq|solarsystem).nasa.gov$" name=".*" />


	<rule from="^http://(corts\.hq|solarsystem)\.nasa\.gov/"
		to="https://$1.nasa.gov/" />

</ruleset>
