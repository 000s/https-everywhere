<ruleset name="NASA">

	<target host="corts.hq.nasa.gov" />
	<target host="solarsystem.nasa.gov" />


	<securecookie host="^(corts\.hq|solarsystem).nasa.gov$" name=".*" />


	<rule from="^http://(corts\.hq|solarsystem)\.nasa\.gov/"
		to="https://$1.nasa.gov/" />

</ruleset>
