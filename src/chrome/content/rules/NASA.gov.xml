<ruleset name="NASA">

	<target host="*.nasa.gov" />


	<securecookie host="^.*\.nasa\.gov$" name=".+" />


	<rule from="^http://(airbornescience|cloud1\.arc|astrobiology|ws1\.ems\.eosdis|esdpubs|(?:www\.)?(?:espo|spacecomm)|espoarchive|rt\.grc|heasarc\.gsfc|(?:corts|www)\.hq|solarsystem)\.nasa\.gov/"
		to="https://$1.nasa.gov/" />

	<rule from="^http://eyes\.nasa\.gov/"
		to="https://dkaj27004uji9.cloudfront.net/" />

	<rule from="^https?://heasarc\.nasa\.gov/"
		to="https://heasarc.gsfc.nasa.gov/" />

</ruleset>