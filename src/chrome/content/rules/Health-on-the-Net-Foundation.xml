<ruleset name="Health on the Net Foundation">

	<target host="hon.ch"/>
	<target host="www.hon.ch"/>

	<securecookie host="^(.*\.)?hon\.ch$" name=".*"/>

	<!--	cert !valid for !www	-->
	<rule from="^http://hon\.ch/"
		to="https://www.hon.ch/"/>

	<rule from="^http://(services|www)\.hon\.ch/"
		to="https://$1.hon.ch/"/>

</ruleset>
