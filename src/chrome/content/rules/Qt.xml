<!--
	Other Nokia rulesets:

		- Nokia.xml

-->
<ruleset name="Qt">

	<target host="qt-project.org" />
	<target host="*.qt-project.org" />


	<securecookie host="^(.*\.)?qt-project\.org$" name=".*" />

	<!--
		Observed subdomains:

			- bugreports
			- codereview
			- www
				-->
  <exclusion pattern="lists\.qt-project\.org" />
	<rule from="^http://(\w+\.)?qt-project\.org/"
		to="https://qt-project.org/" />

</ruleset>
