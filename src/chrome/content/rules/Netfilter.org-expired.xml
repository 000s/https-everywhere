<!--
	For rules that are on by default, see Netfilter.org.xml.

-->
<ruleset name="Netfilter.org (expired)" default_off="expired, self-signed" platform="cacert">

	<target host="bugzilla.netfilter.org" />
	<target host="lists.netfilter.org" />


	<rule from="^http://bugzilla\.netfilter\.org/(?:bugzilla3/)?"
		to="https://bugzilla.netfilter.org/bugzilla3/" />

	<rule from="^http://lists\.netfilter\.org/"
		to="https://lists.netfilter.org/" />

</ruleset>
