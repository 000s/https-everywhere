<ruleset name="Commission Junction">

	<target host="www.apmebf.com"/>
	<target host="www.awltovhc.com"/>
	<target host="cj.com"/>
	<target host="*.cj.com"/>
	<target host="www.lduhtrp.net"/>
	<target host="www.qksz.net" />
	<target host="www.qksrv.net" />

	<securecookie host="^(.*\.)?(apmebf|cj)\.com$" name=".*"/>

	<rule from="^http://www\.apmebf\.com/$"
		to="https://www.cj.com/privacy.html"/>

	<rule from="^http://www\.awltovhc\.com/"
		to="https://www.awltovhc.com/" />

	<rule from="^http://cj\.com/"
		to="https://www.cj.com/"/>

	<rule from="^http://(members|signup|www)\.cj\.com/"
		to="https://$1.cj.com/"/>

	<rule from="^http://www\.lduhtrp\.net/"
		to="https://www.lduhtrp.net/"/>

	<rule from="^https?://www\.qks(?:rv|z)\.net/"
		to="https://www.qksrv.net/" />

</ruleset>
