<ruleset name="Memorial University">
	<target host="*.mun.ca" />

	<securecookie host="^online\.mun\.ca$" name=".+" />

	<rule from="^http://(www\.(?:[^.]+\.)?)mun\.ca/"
		to="https://$1mun.ca/" />
	<rule from="^http://online\.mun\.ca/"
		to="https://online.mun.ca/" />
	<rule from="^http://((?:[^.]+\.)?)delts\.mun\.ca/"
		to="https://$1delts.mun.ca/" />
</ruleset>