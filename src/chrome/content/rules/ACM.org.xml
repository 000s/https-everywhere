<!-- acm.org lacks proper ssl for the following domains
http://plone.acm.org
http://portalparts.acm.org.
http://jobs.acm.org <- sends to jobtarget.com
http://awards.acm.org <- stacktrace for coldfusion
http://librarians.acm.org/ 
http://mags.acm.org/ 
-->
<ruleset name="ACM.org">
	<target host="campus.acm.org" />
	<target host="portal.acm.org" />
	<target host="techpack.acm.org" />
	<target host="acm.org" />
	<target host="www.acm.org" />
	<target host="dl.acm.org" />

	<rule from="^http://campus\.acm\.org/" to="https://campus.acm.org/"/>
	<rule from="^http://portal\.acm\.org/" to="https://portal.acm.org/"/>
	<rule from="^http://techpack\.acm\.org/" to="https://techpack.acm.org/"/>
	<rule from="^http://acm\.org/" to="https://www.acm.org/"/>
	<rule from="^http://www\.acm\.org/" to="https://www.acm.org/"/>
        <rule from="^http://dl\.acm\.org/" to="https://dl.acm.org/"/>
</ruleset>
