<!--
	For problematic rules, see ACM.org-mismatches.xml.


	acm.org lacks proper ssl for the following domains

		- awards		<- stacktrace for coldfusion
		- jobs			<- sends to jobtarget.com
		- librarians
		- mags
		- plone
		- portalparts

-->
<ruleset name="ACM.org (partial)">

	<target host="acm.org" />
	<target host="*.acm.org" />


	<securecookie host="^.*\.acm\.org$" name=".*" />


	<rule from="^http://((?:campus|dl|portal|queue|techpack)\.)?acm\.org/"
		to="https://$1acm.org/" />

</ruleset>
