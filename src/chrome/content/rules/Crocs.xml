<!--
	Cert only matches www

-->
<ruleset name="Crocs">

	<target host="crocs.com" />
	<target host="*.crocs.com" />


	<securecookie host="^www\.crocs\.com$" name=".+" />


	<rule from="^https?://(?:www\.)?crocs\.com/"
		to="https://www.crocs.com/" />

	<rule from="^http://images\.crocs\.com/"
		to="https://images.crocs.com/" />

</ruleset>