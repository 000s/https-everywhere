<ruleset name="temp-mail.org">

	<target host="temp-mail.org" />
	<target host="www.temp-mail.org" />

	<target host="temp-mail.ru" />
	<target host="www.temp-mail.ru" />
	<target host="api.temp-mail.ru" />


	<rule from="^http://www\.temp-mail\.ru/"
		to="https://temp-mail.ru/" />

	<rule from="^http://www\.temp-mail\.org/"
		to="https://temp-mail.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>

