<!--
	For rules that are on by default, see Constant-Contact.xml.

-->
<ruleset name="Constant Contact (mismatches)" default_off="mismatched">

	<target host="blogs.constantcontact.com" />
		<exclusion pattern="^http://blogs\.constantcontact\.com/(?!wp-admin(?:$|[?/])|wp-content/|wp-includes/|wp-login\.php)" />


	<rule from="^http://blogs\.constantcontact\.com/"
		to="https://blogs.constantcontact.com/" />

</ruleset>
