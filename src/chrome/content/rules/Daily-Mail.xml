<ruleset name="Daily Mail (partial)" default_off="Akamai certificate">

	<target host="dailymail.co.uk" />
	<target host="scripts.dailymail.co.uk" />
	<target host="www.dailymail.co.uk" />

	<rule from="^http://(www\.)?dailymail\.co\.uk/i/"
		to="https://www.dailymail.co.uk/i/" />

	<rule from="^http://scripts\.dailymail\.co\.uk/"
		to="https://scripts.dailymail.co.uk/" />
</ruleset>
