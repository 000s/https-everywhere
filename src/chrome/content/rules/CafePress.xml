<ruleset name="CafePress (partial)">

	<target host="cafepress.co.uk"/>
	<target host="content.cafepress.co.uk"/>
	<target host="members.cafepress.co.uk"/>
	<target host="www.cafepress.co.uk"/>

	<rule from="^http://(www\.)?cafepress\.co\.uk/content/" to="https://www.cafepress.co.uk/content/"/>

	<rule from="^http://(content|members)\.cafepress\.co\.uk/" to="https://$1.cafepress.co.uk/"/>

</ruleset>
