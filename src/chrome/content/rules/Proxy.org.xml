<!--
	Fully covered subdomains:

		- (www.)


	Mixed content:

		- Images from ^proxy.org *

	* Secured by us, doesn't trip MCB anyway

-->
<ruleset name="Proxy.org">

	<target host="proxy.org"/>
	<target host="*.proxy.org"/>

	<!--	encountered:
			- proxy.org
			- .proxy.org		-->
	<securecookie host="^(.*\.)?proxy\.org$" name=".*"/>

	<rule from="^http://(www\.)?proxy\.org/"
		to="https://$1proxy.org/"/>

</ruleset>
