<ruleset name="IsoHunt">
    <target host="isohunt.com" />
    <target host=".isohunt.com" />
    <target host="www.isohunt.com" />
    <target host="ca.isohunt.com" />

    <rule from="^http://(?:www\.)?isohunt\.com/"
	    to="https://isohunt.com/"/>
    <rule from="^http://ca\.isohunt\.com/"
	    to="https://ca.isohunt.com/"/>

    <securecookie host="^(ca)?\.isohunt\.com$" name=".*"/>
</ruleset>
