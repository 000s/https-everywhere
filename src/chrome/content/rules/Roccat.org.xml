<!--
	Mixed content:

		- css on www from www *
		- Images on www from www *


	mixedcontent due to css on www from www.

-->
<ruleset name="Roccat.org (false MCB)" default_off="invalid certificate" platform="mixedcontent">
  <target host="roccat.org" />
  <target host="www.roccat.org" />

  <rule from="^http://(?:www\.)?roccat\.org/" to="https://www.roccat.org/" />
</ruleset>
