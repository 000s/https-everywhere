<ruleset name="is.gd">
    <target host="is.gd" />
    <target host="v.gd" />

    <securecookie host="is\.gd$" name=".*" />
    <securecookie host="v\.gd$" name=".*" />

    <rule from="^https?://is\.gd/"
        to="https://is.gd/" />
    <rule from="^https?://v\.gd/"
        to="https://v.gd/" />
</ruleset>
