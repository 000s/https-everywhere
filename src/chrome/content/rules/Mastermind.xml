<ruleset name="Mastermind (partial)">

	<target host="mastermindpro.com" />
	<target host="*.mastermindpro.com" />


	<!--	- Cert: secure2.mastermindpro.com
		- At least some pages redirect from !www to www
					-->
	<rule from="^http://(www\.)?mastermindpro\.com/"
		to="https://$1mastermindpro.com/" />

	<rule from="^http://secure2\.mastermindpro\.com/"
