<!--
	Problematic subdomains:

		- www	(cert only matches ^dolimg.com)


	Web bugs.

-->
<ruleset name="Dolimg.com">

	<target host="dolimg.com" />
	<target host="*.dolimg.com" />


	<rule from="^http://(?:www\.)?dolimg\.com/"
		to="https://dolimg.com/" />

	<rule from="^http://s\.dolimg\.com/"
		to="https://s.dolimg.com/" />

</ruleset>