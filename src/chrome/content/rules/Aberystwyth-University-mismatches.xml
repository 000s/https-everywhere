<!--	See Aberystwyth-University.xml also
-->
<ruleset name="Aberystwyth University (mismatches)" default_off="Certificate mismatch, self-signed">

	<target host="connect.aber.ac.uk" />
	<target host="abersfx.hosted.exlibrisgroup.com" />

	<rule from="^http://connect\.aber\.ac\.uk/"
		to="https://connect.aber.ac.uk/" />

	<rule from="^http://abersfx\.hosted\.exlibrisgroup\.com/"
		to="https://abersfx.hosted.exlibrisgroup.com/" />

</ruleset>
