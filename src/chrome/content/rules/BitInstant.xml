<!--
	Some of blog.bitinstant.com is handled in Squarespace-clients.xml.

-->
<ruleset name="BitInstant">

	<target host="bitinstant.com" />
	<target host="*.bitinstant.com" />


	<rule from="^http://(www\.)?bitinstant\.com/"
		to="https://$1bitinstant.com/" />

	<!--	Cert: *.squarespace.com	-->
	<rule from="^https?://blog\.bitinstant\.com/universal/"
		to="https://squarespace.com/universal/" />

</ruleset>
