<ruleset name="Suomi.fi">

	<!-- Has a few subdomains which have cookies too -->

	<target host="suomi.fi" />
	<target host="www.suomi.fi" />


	<securecookie host="^[^@:/\.]\.suomi\.fi" name=".*" />


	<rule from="^http://(www\.)?suomi\.fi/" to="https://www.suomi.fi/"/>
	<rule from="^https://suomi\.fi/" to="https://www.suomi.fi/"/>

</ruleset>
