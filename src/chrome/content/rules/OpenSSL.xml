<ruleset name="OpenSSL" default_off="different content between HTTP and HTTPS">
  <target host="openssl.org" />
  <target host="www.openssl.org" />

  <rule from="^http://(?:www\.)?openssl\.org/" to="https://www.openssl.org/"/>
</ruleset>

