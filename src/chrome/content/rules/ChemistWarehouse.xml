<!--
	For chemistwarehouse.com.au

		Works:

			- secure

		Problematic:

			- www * & **
			- $ **

		* cert mismatch
		** redirects to http
-->
<ruleset name="Chemist Warehouse">
  <target host="chemistwarehouse.com.au" />
  <target host="*.chemistwarehouse.com.au" />

  <rule from="^http://secure\.chemistwarehouse\.com\.au/"
          to="https://secure.chemistwarehouse.com.au/" />
</ruleset>