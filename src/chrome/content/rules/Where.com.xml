<!--
	Ads.

	For other PayPal coverage. see PayPal.xml.


	Fully covered subdomains:

		- ad
		- (www.)


	Observed cookie domains:

		- ad
		- upstream

-->
<ruleset name="Where.com">

	<target host="where.com" />
	<target host="*.where.com" />


	<securecookie host="^ad\.where\.com$" name=".+" />


	<rule from="^http://(ad\.|www\.)?where\.com/"
		to="https://$1where.com/" />

</ruleset>
