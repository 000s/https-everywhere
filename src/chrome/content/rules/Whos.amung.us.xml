<!--
	whos.amung.us Inc


	Nonfunctional subdomains:

		- (www.)	(403, valid cert)


	Problematic subdomains:

		- whos		(some paths 403)


	Partially covered subdomains:

		- whos


	Fully covered subdomains:

		- assets

-->
<ruleset name="whos.amung.us (partial)">

	<target host="*.amung.us" />
		<!--
			403:
				-->
		<!--exclusion pattern="^http://whos\.amung\.us/+($|\?|(about|blog|contact|learnmore|legal|pro(?!/login/)|showcase)($|[?/])|favicon\.ico|widget/)" /-->


	<rule from="^http://whos\.amung\.us/pro/login/"
		to="https://whos.amung.us/pro/login/" />

	<rule from="^http://assets\.amung\.us/"
		to="https://assets.amung.us/" />

</ruleset>
