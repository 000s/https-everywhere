<ruleset name="Freelancer">

	<target host="freelancer.com" />
	<target host="*.freelancer.com" />
	<target host="freelancer.co.uk" />
	<target host="*.freelancer.co.uk" />


	<securecookie host="^(.+\.)?freelancer\.co(m|\.uk)$" name=".*" />


	<rule from="^http://(?:www\.)?freelancer\.co(m|\.uk)/"
		to="https://www.freelancer.co$1/" />

	<rule from="^http://(cdn\d+)\.freelancer\.co(m|\.uk)/"
		to="https://$1.freelancer.co$2/" />

</ruleset>
