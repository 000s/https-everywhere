<ruleset name="Vimeo">
  <target host="vimeo.com" />
  <target host="player.vimeo.com"/>
  <target host="www.vimeo.com" />

  <rule from="^http://player\.vimeo\.com/"
	  to="https://player.vimeo.com/"/>

  <!-- secure login form -->
  <rule from="^http://(?:www\.)?vimeo\.com/log_in" to="https://secure.vimeo.com/log_in"/>
</ruleset>

