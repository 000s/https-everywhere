<ruleset name="Tchibo">
	<target host="*.tchibo.de"/>
	<target host="*.tchibo.ch"/>
	<target host="*.fr.tchibo.ch"/>
	<target host="*.eduscho.at"/>
	<target host="*.tchibo.sk"/>
	<target host="*.tchibo.com.tr"/>
	<target host="*.tchibo.pl"/>
	<target host="*.tchibo.cz"/>
	<target host="*.tchibo.hu"/>
	
	<rule from="^http://([^/:@]*)\.tchibo\.de/"
		to="https://$1.tchibo.de/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.ch/"
		to="https://$1.tchibo.ch/"/>
	<rule from="^http://([^/:@]*)\.fr.tchibo\.ch/"
		to="https://$1.fr.tchibo.ch/"/>
	<rule from="^http://([^/:@]*)\.eduscho\.at/"
		to="https://$1.eduscho.at/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.sk/"
		to="https://$1.tchibo.sk/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.com\.tr/"
		to="https://$1.tchibo.com.tr/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.pl/"
		to="https://$1.tchibo.pl/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.cz/"
		to="https://$1.tchibo.cz/"/>
	<rule from="^http://([^/:@]*)\.tchibo\.hu/"
		to="https://$1.tchibo.hu/"/>
</ruleset>
