<ruleset name="Digital Advertising Alliance">

	<target host="aboutads.info"/>
	<!--	* for cross-domain cookie	-->
	<target host="*.aboutads.info"/>

	<securecookie host="^(.*\.)?aboutads\.info$" name=".*"/>

	<!--	cert !valid for !www	-->
	<rule from="^http://(?:www\.)?aboutads\.info/"
		to="https://www.aboutads.info/"/>

</ruleset>
