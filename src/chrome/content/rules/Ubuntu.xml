<!--
	Nonfunctional domains:

		- cdimage.ubuntu.com
		- odm.ubuntu.com		(cert: testcases.qa.ubuntu.com_https; 301s there)
		- testcases.qa.ubuntu.com	(self-signed, 301s to http)
		- pad.ubuntu.com
		- summit.ubuntu.com
		- (www.)ubuntu.com		(times out)
		- (www.)ubuntuforums.org	(times out)

-->
<ruleset name="Ubuntu (partial)">

	<target host="*.ubuntu.com" />
	<target host="myapps.developer.ubuntu.com" />
	<target host="ubuntuone.com" />
	<target host="www.ubuntuone.com" />


	<securecookie host="^apps\.ubuntu\.com$" name=".*" />


	<rule from="^http://(apps|cloud|(?:myapps\.)?developer|help|uds|uec-images|wiki|one)\.ubuntu\.com/"
		to="https://$1.ubuntu.com/" />

	<rule from="^https?://(?:www\.)?ubuntuone\.com/"
		to="https://one.ubuntu.com/" />

</ruleset>
