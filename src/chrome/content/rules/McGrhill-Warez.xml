<ruleset name="McGrhill Warez" default_off="expired, mismatch, self-signed">

	<!--	Cert: *.lxlabs.com	-->

	<!--	Guessing that this is related
		related to speeddl.com,
		only by domain names.	-->
	<target host="cafeddl.com" />
	<target host="www.cafeddl.com" />
	<target host="mcgrhill-warez.com" />
	<target host="www.mcgrhill-warez.com" />
	<!--	Seems identical to the above.	-->
	<target host="speeddl.com" />
	<target host="www.speeddl.com" />


	<securecookie host="^(cafeddl|mcgrhill-warez)\.com$" name=".*" />


	<rule from="^http://(?:www\.)?(mcgrhill-warez|(caf|spe)eddl)\.com/"
		to="https://$1.com/" />

</ruleset>
