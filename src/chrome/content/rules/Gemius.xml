<ruleset name="Gemius">

	<target host="gemius.pl" />
	<target host="pro.hit.gemius.pl" />
	<target host="www.gemius.pl" />


	<securecookie host="^www\.gemius\.pl$" name=".*" />


	<!--	Cert only matches *.gemius.pl
						-->
	<rule from="^https?://(?:www\.)?gemius\.pl/"
		to="https://www.gemius.pl/" />

	<!--	Included on 3rd-party websites.
						-->
	<rule from="^http://pro\.hit\.gemius\.pl/"
		to="https://pro.hit.gemius.pl/" />

</ruleset>
