<!--
	For other Cupid plc coverage, see Cupid_plc.com.xml


	Partially covered domains:

		- (www.)cupid.com
		- ed.cupid.com *

	* $ redirects to http


	Fully covered domains:

		- (www.)becoquin.com	(^ â†’ www)
		- cdn.cupid.com

-->
<ruleset name="Cupid (partial)">

	<target host="becoquin.com"/>
	<target host="www.becoquin.com"/>
	<target host="cupid.com"/>
	<target host="*.cupid.com"/>
		<exclusion pattern="^http://ed\.cupid\.com/(?!ext\.php)" />


	<securecookie host="^(.*\.)?becoquin\.com$" name=".*"/>
	<securecookie host="^\.cupid\.com$" name="^TRACK_\w+$" />


	<rule from="^http://(?:www\.)?becoquin\.com/"
		to="https://www.becoquin.com/"/>

	<rule from="^http://(cdn|ed)\.cupid\.com/"
		to="https://$1.cupid.com/" />

	<rule from="^http://(?:www\.)?cupid\.com/(aff|oth)\.php"
		to="https://www.cupid.com/$1.php"/>

</ruleset>
