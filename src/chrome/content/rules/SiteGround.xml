<ruleset name="SiteGround (partial)">

	<target host="siteground.com"/>
	<target host=".siteground.com"/>
	<target host="kb.siteground.com"/>
	<target host="www.siteground.com"/>

	<rule from="^http://(www\.)?siteground\.com/"
		to="https://www.siteground.com/"/>

	<rule from="^http://kb\.siteground\.com/"
		to="https://kb.siteground.com/"/>

	<securecookie host="^(www)?\.siteground\.com$" name=".*"/>

</ruleset>
