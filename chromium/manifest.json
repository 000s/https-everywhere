{
    "manifest_version": 2,
    "background": {
        "scripts": ["util.js", "jsuri-1.1.1.js", "rule_list.js", "rules.js", "background.js"]
    },
    "description": "Automatically encrypt your connection to major websites.", 
    "homepage_url": "https://www.eff.org/https-everywhere", 
    "icons": {
        "128": "icon128.png", 
        "16": "icon16.png", 
        "48": "icon48.png"
    }, 
    "minimum_chrome_version": "18",
    "name": "HTTPS Everywhere", 
    "page_action": {
        "default_icon": "icon48.png", 
        "default_popup": "popup.html", 
        "default_title": "HTTPS Everywhere"
    }, 
    "permissions": [
        "webRequest", 
        "webRequestBlocking", 
        "tabs", 
        "cookies", 
        "<all_urls>"
    ], 
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "2012.6.21",
    "author": {
      "email": "eff.software.projects@gmail.com"
    }
}
